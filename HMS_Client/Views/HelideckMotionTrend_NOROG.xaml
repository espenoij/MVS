<UserControl
    x:Class="HMS_Client.HelideckMotionTrend_NOROG"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:telerik="http://schemas.telerik.com/2008/xaml/presentation"
    xmlns:local="clr-namespace:HMS_Client"
    mc:Ignorable="d"
    d:DesignHeight="600"
    d:DesignWidth="710">

    <UserControl.Resources>

        <local:LabelAbsConverter
            x:Key="LabelAbsConverter" />

    </UserControl.Resources>

    <!--Helideck Motion Trend-->
    <telerik:GroupBox
        Header="HELIDECK MOTION TREND"
        Margin="8">

        <telerik:RadTabControl
            Grid.Column="0"
            Grid.Row="0"
            Grid.ColumnSpan="3"
            Margin="8,8,8,-8"
            HeaderBackground="{StaticResource ColorHorizontalBarBackground}">

            <telerik:RadTabItem
                MaxHeight="{StaticResource tabBarHeight}">
                <telerik:RadTabItem.Header>

                    <StackPanel
                        Orientation="Horizontal">

                        <Image
                            Width="24"
                            Source="..\Icons\outline_schedule_white_48dp.png" />

                        <TextBlock
                            Foreground="White"
                            FontSize="{StaticResource fontSizeTab}"
                            Text="20-minutes"
                            Margin="8,4,0,0" />

                    </StackPanel>

                </telerik:RadTabItem.Header>

                <Grid
                    Margin="8">

                    <Grid.ColumnDefinitions>
                        <ColumnDefinition
                            Width="4*" />
                        <ColumnDefinition
                            Width="18*" />
                        <ColumnDefinition
                            Width="6*" />
                    </Grid.ColumnDefinitions>

                    <Grid.RowDefinitions>
                        <RowDefinition
                            Height="4*" />
                        <RowDefinition
                            Height="20" />
                        <RowDefinition
                            Height="4*" />
                        <RowDefinition
                            Height="20" />
                        <RowDefinition
                            Height="4*" />
                        <RowDefinition
                            Height="20" />
                        <RowDefinition
                            Height="4*" />
                        <RowDefinition
                            Height="20" />
                        <RowDefinition
                            Height="4*" />
                        <RowDefinition
                            Height="20" />
                    </Grid.RowDefinitions>

                    <!--Pitch Title-->
                    <Grid
                        Grid.Column="0"
                        Grid.Row="0">

                        <Grid.RowDefinitions>
                            <RowDefinition
                                Height="2*" />
                            <RowDefinition
                                Height="*" />
                            <RowDefinition
                                Height="3*" />
                        </Grid.RowDefinitions>


                        <Viewbox
                            Grid.Row="1"
                            StretchDirection="Both"
                            Stretch="Uniform"
                            HorizontalAlignment="Center">

                            <TextBlock
                                Text="Pitch" />

                        </Viewbox>

                    </Grid>

                    <!--Pitch Chart-->
                    <telerik:RadCartesianChart
                        Grid.Column="1"
                        Grid.Row="0"
                        Name="chartPitch20m"
                        Margin="6,-6,0,-6">

                        <telerik:LineSeries
                            ZIndex="200"
                            Stroke="{StaticResource ColorGraphLine}"
                            CategoryBinding="timestamp"
                            ValueBinding="graphData">

                            <telerik:LineSeries.RenderOptions>
                                <telerik:XamlRenderOptions
                                    DefaultVisualsRenderMode="Batch" />
                            </telerik:LineSeries.RenderOptions>

                        </telerik:LineSeries>

                        <telerik:RadCartesianChart.HorizontalAxis>
                            <telerik:DateTimeContinuousAxis
                                x:Name="chartPitchHorizontalAxis20m"
                                MajorStepUnit="Minute"
                                MajorStep="5"
                                ShowLabels="False"
                                PlotMode="OnTicks"
                                TickOrigin="{Binding alignmentTime}"
                                Minimum="{Binding alignmentTimeMin20m}"
                                Maximum="{Binding alignmentTimeMax}" />
                        </telerik:RadCartesianChart.HorizontalAxis>

                        <telerik:RadCartesianChart.VerticalAxis>
                            <telerik:LinearAxis
                                x:Name="chartPitchVerticalAxis20m"
                                Maximum="{Binding pitchChartAxisMax20m}"
                                Minimum="{Binding pitchChartAxisMin20m}">

                                <telerik:LinearAxis.LabelTemplate>
                                    <DataTemplate>
                                        <TextBlock
                                            Text="{Binding Converter={StaticResource LabelAbsConverter}}" />
                                    </DataTemplate>
                                </telerik:LinearAxis.LabelTemplate>

                            </telerik:LinearAxis>
                        </telerik:RadCartesianChart.VerticalAxis>

                        <telerik:RadCartesianChart.Grid>
                            <telerik:CartesianChartGrid
                                MajorLinesVisibility="XY" />
                        </telerik:RadCartesianChart.Grid>

                        <telerik:RadCartesianChart.Annotations>

                            <telerik:CartesianGridLineAnnotation
                                Axis="{Binding ElementName=chartPitchVerticalAxis20m}"
                                Value="{Binding pitchChartAnnotationRedTopMin20m}"
                                Stroke="{StaticResource ColorRed}"
                                StrokeThickness="2" />

                            <telerik:CartesianGridLineAnnotation
                                Axis="{Binding ElementName=chartPitchVerticalAxis20m}"
                                Value="{Binding pitchChartAnnotationRedBottomMin20m}"
                                Stroke="{StaticResource ColorRed}"
                                StrokeThickness="2" />

                        </telerik:RadCartesianChart.Annotations>

                    </telerik:RadCartesianChart>

                    <!--Pitch chart labels-->
                    <Grid
                        Grid.Column="1"
                        Grid.Row="1"
                        Margin="10,0,-6,0">

                        <Grid.ColumnDefinitions>
                            <ColumnDefinition
                                Width="*" />
                            <ColumnDefinition
                                Width="*" />
                            <ColumnDefinition
                                Width="*" />
                            <ColumnDefinition
                                Width="*" />
                            <ColumnDefinition
                                Width="*" />
                            <ColumnDefinition
                                Width="*" />
                            <ColumnDefinition
                                Width="*" />
                            <ColumnDefinition
                                Width="*" />
                        </Grid.ColumnDefinitions>

                        <Viewbox
                            Grid.Column="0"
                            Grid.ColumnSpan="2"
                            StretchDirection="Both"
                            Stretch="Uniform"
                            HorizontalAlignment="Left">

                            <Label
                                Content="-20 mins" />

                        </Viewbox>

                        <Viewbox
                            Grid.Column="1"
                            Grid.ColumnSpan="2"
                            StretchDirection="Both"
                            Stretch="Uniform"
                            HorizontalAlignment="Center">

                            <Label
                                Content="-15" />

                        </Viewbox>

                        <Viewbox
                            Grid.Column="3"
                            Grid.ColumnSpan="2"
                            StretchDirection="Both"
                            Stretch="Uniform"
                            HorizontalAlignment="Center">

                            <Label
                                Content="-10" />

                        </Viewbox>

                        <Viewbox
                            Grid.Column="5"
                            Grid.ColumnSpan="2"
                            StretchDirection="Both"
                            Stretch="Uniform"
                            HorizontalAlignment="Center">

                            <Label
                                Content="-5" />

                        </Viewbox>

                        <Viewbox
                            Grid.Column="10"
                            StretchDirection="Both"
                            Stretch="Uniform"
                            HorizontalAlignment="Right">

                            <Label
                                Content="0" />

                        </Viewbox>

                    </Grid>

                    <!--Pitch Max-->
                    <Grid
                        Grid.Column="2"
                        Grid.Row="0"
                        Margin="16,0,0,0">

                        <Grid.RowDefinitions>
                            <RowDefinition
                                Height="*" />
                            <RowDefinition
                                Height="*" />
                            <RowDefinition
                                Height="*" />
                            <RowDefinition
                                Height="*" />
                            <RowDefinition
                                Height="*" />
                            <RowDefinition
                                Height="2*" />
                        </Grid.RowDefinitions>

                        <Grid
                            Grid.Row="1"
                            Name="gridMaxPitchUp20m">

                            <Grid.ColumnDefinitions>
                                <ColumnDefinition
                                    Width="4*" />
                                <ColumnDefinition
                                    Width="2*" />
                            </Grid.ColumnDefinitions>

                            <Viewbox
                                Grid.Column="0"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Left">

                                <TextBlock
                                    Text="Max Pitch" />

                            </Viewbox>

                            <Viewbox
                                Grid.Column="1"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Right">

                                <TextBlock
                                    Text="{Binding Path=pitchMaxUp20mString2}"
                                    FontSize="{StaticResource fontSizeData}" />

                            </Viewbox>

                        </Grid>

                        <Grid
                            Grid.Row="2"
                            Name="gridMaxPitchDown20m">

                            <Grid.ColumnDefinitions>
                                <ColumnDefinition
                                    Width="4*" />
                                <ColumnDefinition
                                    Width="2*" />
                            </Grid.ColumnDefinitions>

                            <Viewbox
                                Grid.Column="1"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Right">

                                <TextBlock
                                    Text="{Binding Path=pitchMaxDown20mString2}"
                                    FontSize="{StaticResource fontSizeData}" />

                            </Viewbox>

                        </Grid>

                        <Grid
                            Grid.Row="4">

                            <Grid.ColumnDefinitions>
                                <ColumnDefinition
                                    Width="4*" />
                                <ColumnDefinition
                                    Width="2*" />
                            </Grid.ColumnDefinitions>

                            <Viewbox
                                Grid.Column="0"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Left">

                                <TextBlock
                                    Text="Limit" />

                            </Viewbox>

                            <Viewbox
                                Grid.Column="1"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Right">

                                <TextBlock
                                    Text="{Binding Path=pitchRollLimitString}"
                                    FontSize="{StaticResource fontSizeData}" />

                            </Viewbox>

                        </Grid>

                    </Grid>
                    
                    <Separator
                        Grid.Column="0"
                        Grid.Row="1"
                        Grid.ColumnSpan="3"
                        VerticalAlignment="Bottom" />

                    <!--Roll Title-->
                    <Grid
                        Grid.Column="0"
                        Grid.Row="2">

                        <Grid.RowDefinitions>
                            <RowDefinition
                                Height="2*" />
                            <RowDefinition
                                Height="*" />
                            <RowDefinition
                                Height="3*" />
                        </Grid.RowDefinitions>


                        <Viewbox
                            Grid.Row="1"
                            StretchDirection="Both"
                            Stretch="Uniform"
                            HorizontalAlignment="Center">

                            <TextBlock
                                Text="Roll" />

                        </Viewbox>

                    </Grid>

                    <!--Roll chart-->
                    <telerik:RadCartesianChart
                        Grid.Column="1"
                        Grid.Row="2"
                        Name="chartRoll20m"
                        Margin="6,-6,0,-6">

                        <!--Roll-->
                        <telerik:LineSeries
                            ZIndex="200"
                            Stroke="{StaticResource ColorGraphLine}"
                            CategoryBinding="timestamp"
                            ValueBinding="graphData">

                            <telerik:LineSeries.RenderOptions>
                                <telerik:XamlRenderOptions
                                    DefaultVisualsRenderMode="Batch" />
                            </telerik:LineSeries.RenderOptions>

                        </telerik:LineSeries>

                        <telerik:RadCartesianChart.HorizontalAxis>
                            <telerik:DateTimeContinuousAxis
                                MajorStepUnit="Minute"
                                MajorStep="5"
                                ShowLabels="False"
                                PlotMode="OnTicks"
                                TickOrigin="{Binding alignmentTime}"
                                Minimum="{Binding alignmentTimeMin20m}"
                                Maximum="{Binding alignmentTimeMax}" />
                        </telerik:RadCartesianChart.HorizontalAxis>

                        <telerik:RadCartesianChart.VerticalAxis>
                            <telerik:LinearAxis
                                x:Name="chartRollVerticalAxis20m"
                                Maximum="{Binding rollChartAxisMax20m}"
                                Minimum="{Binding rollChartAxisMin20m}">

                                <telerik:LinearAxis.LabelTemplate>
                                    <DataTemplate>
                                        <TextBlock
                                            Text="{Binding Converter={StaticResource LabelAbsConverter}}" />
                                    </DataTemplate>
                                </telerik:LinearAxis.LabelTemplate>

                            </telerik:LinearAxis>

                        </telerik:RadCartesianChart.VerticalAxis>

                        <telerik:RadCartesianChart.Grid>
                            <telerik:CartesianChartGrid
                                MajorLinesVisibility="XY" />
                        </telerik:RadCartesianChart.Grid>

                        <telerik:RadCartesianChart.Annotations>

                            <telerik:CartesianGridLineAnnotation
                                Axis="{Binding ElementName=chartRollVerticalAxis20m}"
                                Value="{Binding rollChartAnnotationRedTopMin20m}"
                                Stroke="{StaticResource ColorRed}"
                                StrokeThickness="2" />

                            <telerik:CartesianGridLineAnnotation
                                Axis="{Binding ElementName=chartRollVerticalAxis20m}"
                                Value="{Binding rollChartAnnotationRedBottomMin20m}"
                                Stroke="{StaticResource ColorRed}"
                                StrokeThickness="2" />

                        </telerik:RadCartesianChart.Annotations>

                    </telerik:RadCartesianChart>
                    
                    <!--Roll chart labels-->
                    <Grid
                        Grid.Column="1"
                        Grid.Row="3"
                        Margin="10,0,-6,0">

                        <Grid.ColumnDefinitions>
                            <ColumnDefinition
                                Width="*" />
                            <ColumnDefinition
                                Width="*" />
                            <ColumnDefinition
                                Width="*" />
                            <ColumnDefinition
                                Width="*" />
                            <ColumnDefinition
                                Width="*" />
                            <ColumnDefinition
                                Width="*" />
                            <ColumnDefinition
                                Width="*" />
                            <ColumnDefinition
                                Width="*" />
                        </Grid.ColumnDefinitions>

                        <Viewbox
                            Grid.Column="0"
                            Grid.ColumnSpan="2"
                            StretchDirection="Both"
                            Stretch="Uniform"
                            HorizontalAlignment="Left">

                            <Label
                                Content="-20 mins" />

                        </Viewbox>

                        <Viewbox
                            Grid.Column="1"
                            Grid.ColumnSpan="2"
                            StretchDirection="Both"
                            Stretch="Uniform"
                            HorizontalAlignment="Center">

                            <Label
                                Content="-15" />

                        </Viewbox>

                        <Viewbox
                            Grid.Column="3"
                            Grid.ColumnSpan="2"
                            StretchDirection="Both"
                            Stretch="Uniform"
                            HorizontalAlignment="Center">

                            <Label
                                Content="-10" />

                        </Viewbox>

                        <Viewbox
                            Grid.Column="5"
                            Grid.ColumnSpan="2"
                            StretchDirection="Both"
                            Stretch="Uniform"
                            HorizontalAlignment="Center">

                            <Label
                                Content="-5" />

                        </Viewbox>

                        <Viewbox
                            Grid.Column="10"
                            StretchDirection="Both"
                            Stretch="Uniform"
                            HorizontalAlignment="Right">

                            <Label
                                Content="0" />

                        </Viewbox>

                    </Grid>

                    <!--Roll max-->
                    <Grid
                        Grid.Column="2"
                        Grid.Row="2"
                        Margin="16,0,0,0">

                        <Grid.RowDefinitions>
                            <RowDefinition
                                Height="*" />
                            <RowDefinition
                                Height="*" />
                            <RowDefinition
                                Height="*" />
                            <RowDefinition
                                Height="*" />
                            <RowDefinition
                                Height="*" />
                            <RowDefinition
                                Height="2*" />
                        </Grid.RowDefinitions>

                        <Grid
                            Grid.Row="1"
                            Name="gridMaxRollLeft20m">

                            <Grid.ColumnDefinitions>
                                <ColumnDefinition
                                    Width="4*" />
                                <ColumnDefinition
                                    Width="2*" />
                            </Grid.ColumnDefinitions>

                            <Viewbox
                                Grid.Column="0"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Left">

                                <TextBlock
                                    Text="Max Roll" />

                            </Viewbox>

                            <Viewbox
                                Grid.Column="1"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Right">

                                <TextBlock
                                    Text="{Binding Path=rollMaxLeft20mString2}"
                                    FontSize="{StaticResource fontSizeData}" />

                            </Viewbox>

                        </Grid>

                        <Grid
                            Grid.Row="2"
                            Name="gridMaxRollRight20m">

                            <Grid.ColumnDefinitions>
                                <ColumnDefinition
                                    Width="4*" />
                                <ColumnDefinition
                                    Width="2*" />
                            </Grid.ColumnDefinitions>

                            <Viewbox
                                Grid.Column="1"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Right">

                                <TextBlock
                                    Text="{Binding Path=rollMaxRight20mString2}"
                                    FontSize="{StaticResource fontSizeData}" />

                            </Viewbox>

                        </Grid>

                        <Grid
                            Grid.Row="4">

                            <Grid.ColumnDefinitions>
                                <ColumnDefinition
                                    Width="4*" />
                                <ColumnDefinition
                                    Width="2*" />
                            </Grid.ColumnDefinitions>

                            <Viewbox
                                Grid.Column="0"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Left">

                                <TextBlock
                                    Text="Limit" />

                            </Viewbox>

                            <Viewbox
                                Grid.Column="1"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Right">

                                <TextBlock
                                    Text="{Binding Path=pitchRollLimitString}"
                                    FontSize="{StaticResource fontSizeData}" />

                            </Viewbox>

                        </Grid>

                    </Grid>

                    <Separator
                        Grid.Column="0"
                        Grid.Row="3"
                        Grid.ColumnSpan="3"
                        VerticalAlignment="Bottom" />

                    <!--Inclination Title-->
                    <Grid
                        Grid.Column="0"
                        Grid.Row="4">

                        <Grid.RowDefinitions>
                            <RowDefinition
                                Height="2*" />
                            <RowDefinition
                                Height="*" />
                            <RowDefinition
                                Height="3*" />
                        </Grid.RowDefinitions>

                        <Viewbox
                            Grid.Row="1"
                            StretchDirection="Both"
                            Stretch="Uniform"
                            HorizontalAlignment="Center">

                            <TextBlock
                                Text="Inclination" />

                        </Viewbox>

                    </Grid>

                    <!--Inclination Chart-->
                    <telerik:RadCartesianChart
                        Grid.Column="1"
                        Grid.Row="4"
                        Name="chartInclination20m"
                        Margin="-12,-6,0,-6">

                        <telerik:LineSeries
                            ZIndex="200"
                            Stroke="{StaticResource ColorGraphLine}"
                            CategoryBinding="timestamp"
                            ValueBinding="graphData">

                            <telerik:LineSeries.RenderOptions>
                                <telerik:XamlRenderOptions
                                    DefaultVisualsRenderMode="Batch" />
                            </telerik:LineSeries.RenderOptions>

                        </telerik:LineSeries>

                        <telerik:RadCartesianChart.HorizontalAxis>
                            <telerik:DateTimeContinuousAxis
                                MajorStepUnit="Minute"
                                MajorStep="5"
                                ShowLabels="False"
                                PlotMode="OnTicks"
                                TickOrigin="{Binding alignmentTime}"
                                Minimum="{Binding alignmentTimeMin20m}"
                                Maximum="{Binding alignmentTimeMax}" />
                        </telerik:RadCartesianChart.HorizontalAxis>

                        <telerik:RadCartesianChart.VerticalAxis>
                            <telerik:LinearAxis
                                x:Name="chartInclinationVerticalAxis20m"
                                Maximum="{Binding inclinationChartAxisMax20m}"
                                Minimum="0" />
                        </telerik:RadCartesianChart.VerticalAxis>

                        <telerik:RadCartesianChart.Grid>
                            <telerik:CartesianChartGrid
                                MajorLinesVisibility="XY" />
                        </telerik:RadCartesianChart.Grid>

                        <telerik:RadCartesianChart.Annotations>

                            <telerik:CartesianGridLineAnnotation
                                Axis="{Binding ElementName=chartInclinationVerticalAxis20m}"
                                Value="{Binding inclinationChartAnnotationRedMin20m}"
                                Stroke="{StaticResource ColorRed}"
                                StrokeThickness="2" />

                        </telerik:RadCartesianChart.Annotations>

                    </telerik:RadCartesianChart>
                    
                    <!--Inclination chart labels-->
                    <Grid
                        Grid.Column="1"
                        Grid.Row="5"
                        Margin="10,0,-6,0">

                        <Grid.ColumnDefinitions>
                            <ColumnDefinition
                                Width="*" />
                            <ColumnDefinition
                                Width="*" />
                            <ColumnDefinition
                                Width="*" />
                            <ColumnDefinition
                                Width="*" />
                            <ColumnDefinition
                                Width="*" />
                            <ColumnDefinition
                                Width="*" />
                            <ColumnDefinition
                                Width="*" />
                            <ColumnDefinition
                                Width="*" />
                        </Grid.ColumnDefinitions>

                        <Viewbox
                            Grid.Column="0"
                            Grid.ColumnSpan="2"
                            StretchDirection="Both"
                            Stretch="Uniform"
                            HorizontalAlignment="Left">

                            <Label
                                Content="-20 mins" />

                        </Viewbox>

                        <Viewbox
                            Grid.Column="1"
                            Grid.ColumnSpan="2"
                            StretchDirection="Both"
                            Stretch="Uniform"
                            HorizontalAlignment="Center">

                            <Label
                                Content="-15" />

                        </Viewbox>

                        <Viewbox
                            Grid.Column="3"
                            Grid.ColumnSpan="2"
                            StretchDirection="Both"
                            Stretch="Uniform"
                            HorizontalAlignment="Center">

                            <Label
                                Content="-10" />

                        </Viewbox>

                        <Viewbox
                            Grid.Column="5"
                            Grid.ColumnSpan="2"
                            StretchDirection="Both"
                            Stretch="Uniform"
                            HorizontalAlignment="Center">

                            <Label
                                Content="-5" />

                        </Viewbox>

                        <Viewbox
                            Grid.Column="10"
                            StretchDirection="Both"
                            Stretch="Uniform"
                            HorizontalAlignment="Right">

                            <Label
                                Content="0" />

                        </Viewbox>

                    </Grid>

                    <!--Inclination Max-->
                    <Grid
                        Grid.Column="2"
                        Grid.Row="4"
                        Margin="16,0,0,0">

                        <Grid.RowDefinitions>
                            <RowDefinition
                                Height="*" />
                            <RowDefinition
                                Height="*" />
                            <RowDefinition
                                Height="*" />
                            <RowDefinition
                                Height="*" />
                            <RowDefinition
                                Height="3*" />
                        </Grid.RowDefinitions>

                        <Grid
                            Grid.Row="1"
                            Name="gridMaxInclination20m">

                            <Grid.ColumnDefinitions>
                                <ColumnDefinition
                                    Width="4*" />
                                <ColumnDefinition
                                    Width="2*" />
                            </Grid.ColumnDefinitions>

                            <Viewbox
                                Grid.Column="0"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Left">

                                <TextBlock
                                    Text="Max Inclination" />

                            </Viewbox>

                            <Viewbox
                                Grid.Column="1"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Right">

                                <TextBlock
                                    Text="{Binding Path=inclinationMax20mString}"
                                    FontSize="{StaticResource fontSizeData}" />

                            </Viewbox>

                        </Grid>

                        <Grid
                            Grid.Row="3">

                            <Grid.ColumnDefinitions>
                                <ColumnDefinition
                                    Width="4*" />
                                <ColumnDefinition
                                    Width="2*" />
                            </Grid.ColumnDefinitions>

                            <Viewbox
                                Grid.Column="0"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Left">

                                <TextBlock
                                    Text="Limit" />

                            </Viewbox>

                            <Viewbox
                                Grid.Column="1"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Right">

                                <TextBlock
                                    Text="{Binding Path=inclinationLimitString}"
                                    FontSize="{StaticResource fontSizeData}" />

                            </Viewbox>

                        </Grid>

                    </Grid>

                    <Separator
                        Grid.Column="0"
                        Grid.Row="5"
                        Grid.ColumnSpan="3"
                        VerticalAlignment="Bottom" />

                    <!--Significant Heave Rate Chart-->
                    <telerik:RadCartesianChart
                        Grid.Column="1"
                        Grid.Row="6"
                        Margin="-12,-6,0,-6"
                        Name="chartSignificantHeaveRate20m">

                        <telerik:LineSeries
                            ZIndex="200"
                            Stroke="{StaticResource ColorGraphLine}"
                            CategoryBinding="timestamp"
                            ValueBinding="graphData">

                            <telerik:LineSeries.RenderOptions>
                                <telerik:XamlRenderOptions
                                    DefaultVisualsRenderMode="Batch" />
                            </telerik:LineSeries.RenderOptions>

                        </telerik:LineSeries>

                        <telerik:RadCartesianChart.HorizontalAxis>
                            <telerik:DateTimeContinuousAxis
                                MajorStepUnit="Minute"
                                MajorStep="5"
                                ShowLabels="False"
                                PlotMode="OnTicks"
                                TickOrigin="{Binding alignmentTime}"
                                Minimum="{Binding alignmentTimeMin20m}"
                                Maximum="{Binding alignmentTimeMax}" />
                        </telerik:RadCartesianChart.HorizontalAxis>

                        <telerik:RadCartesianChart.VerticalAxis>
                            <telerik:LinearAxis
                                x:Name="chartSignificantHeaveRateVerticalAxis20m"
                                Maximum="{Binding significantHeaveRateChartAxisMax20m}"
                                Minimum="0" />
                        </telerik:RadCartesianChart.VerticalAxis>

                        <telerik:RadCartesianChart.Grid>
                            <telerik:CartesianChartGrid
                                MajorLinesVisibility="XY" />
                        </telerik:RadCartesianChart.Grid>

                        <telerik:RadCartesianChart.Annotations>

                            <telerik:CartesianGridLineAnnotation
                                Axis="{Binding ElementName=chartSignificantHeaveRateVerticalAxis20m}"
                                Value="{Binding significantHeaveRateChartAnnotationRedMin20m}"
                                Stroke="{StaticResource ColorRed}"
                                StrokeThickness="2" />

                        </telerik:RadCartesianChart.Annotations>

                    </telerik:RadCartesianChart>

                    <!--Significant Heave Rate labels-->
                    <Grid
                        Grid.Column="1"
                        Grid.Row="7"
                        Margin="10,0,-6,0">

                        <Grid.ColumnDefinitions>
                            <ColumnDefinition
                                Width="*" />
                            <ColumnDefinition
                                Width="*" />
                            <ColumnDefinition
                                Width="*" />
                            <ColumnDefinition
                                Width="*" />
                            <ColumnDefinition
                                Width="*" />
                            <ColumnDefinition
                                Width="*" />
                            <ColumnDefinition
                                Width="*" />
                            <ColumnDefinition
                                Width="*" />
                        </Grid.ColumnDefinitions>

                        <Viewbox
                            Grid.Column="0"
                            Grid.ColumnSpan="2"
                            StretchDirection="Both"
                            Stretch="Uniform"
                            HorizontalAlignment="Left">

