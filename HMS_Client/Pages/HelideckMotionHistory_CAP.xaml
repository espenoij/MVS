<UserControl
    x:Class="HMS_Client.HelideckMotionHistory_CAP"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:telerik="http://schemas.telerik.com/2008/xaml/presentation"
    xmlns:sys="clr-namespace:System;assembly=mscorlib"
    xmlns:local="clr-namespace:HMS_Client"
    mc:Ignorable="d"
    d:DesignHeight="1280"
    d:DesignWidth="1920">

    <UserControl.Resources>

        <sys:Double
            x:Key="ChartWidth">1600</sys:Double>

        <sys:Double
            x:Key="ChartHeight">200</sys:Double>

        <local:LabelAbsConverter
            x:Key="LabelAbsConverter" />

    </UserControl.Resources>

    <Grid>

        <!--Helideck Motion History-->
        <telerik:GroupBox
            Header="HELIDECK MOTION HISTORY"
            Margin="8">

            <telerik:RadTabControl
                HeaderBackground="{StaticResource ColorHorizontalBarBackground}"
                Margin="8">

                <!--==========================================================================-->
                <!--20-MINUTES-->
                <!--==========================================================================-->
                <telerik:RadTabItem
                    Name="tab20Minutes">
                    <telerik:RadTabItem.Header>
                        <StackPanel
                            Orientation="Horizontal">
                            <Image
                                Width="24"
                                Source="..\Icons\outline_schedule_white_48dp.png" />
                            <TextBlock
                                Foreground="White"
                                FontSize="{StaticResource fontSizeTab}"
                                Text="20-minutes"
                                Margin="8,4,0,0" />
                        </StackPanel>
                    </telerik:RadTabItem.Header>

                    <Grid
                        Margin="8">

                        <Grid.ColumnDefinitions>
                            <ColumnDefinition
                                Width="4*" />
                            <ColumnDefinition
                                Width="54*" />
                            <ColumnDefinition
                                Width="7*" />
                        </Grid.ColumnDefinitions>

                        <Grid.RowDefinitions>
                            <RowDefinition
                                Height="2.5*" />
                            <RowDefinition
                                Height="30" />
                            <RowDefinition
                                Height="9*" />
                            <RowDefinition
                                Height="30" />
                            <RowDefinition
                                Height="9*" />
                            <RowDefinition
                                Height="30" />
                            <RowDefinition
                                Height="9*" />
                            <RowDefinition
                                Height="30" />
                            <RowDefinition
                                Height="9*" />
                            <RowDefinition
                                Height="30" />
                        </Grid.RowDefinitions>

                        <!--LANDING STATUS-->
                        <Grid
                            Grid.Column="0"
                            Grid.Row="0">

                            <Grid.RowDefinitions>
                                <RowDefinition
                                    Height="*" />
                                <RowDefinition
                                    Height="*" />
                                <RowDefinition
                                    Height="*" />
                            </Grid.RowDefinitions>

                            <Viewbox
                                Grid.Row="1"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Center">

                                <TextBlock
                                    Text="Landing" />

                            </Viewbox>

                            <Viewbox
                                Grid.Row="2"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Center">

                                <TextBlock
                                    Text="Status" />

                            </Viewbox>

                        </Grid>

                        <!--Landing Status Trend 20m-->
                        <Grid
                            x:Name="landingStatusTrendGrid20m"
                            Grid.Row="0"
                            Grid.Column="1">

                            <Grid
                                Margin="20,0,0,0">

                                <Grid.RowDefinitions>
                                    <RowDefinition
                                        Height="*" />
                                    <RowDefinition
                                        Height="28" />
                                </Grid.RowDefinitions>

                                <Viewbox
                                    Grid.Row="0"
                                    StretchDirection="Both"
                                    Stretch="Uniform"
                                    HorizontalAlignment="Center">

                                    <Label
                                        HorizontalAlignment="Center"
                                        Content="{Binding Path=landingStatusTimeString}" />

                                </Viewbox>

                                <Viewbox
                                    Grid.Row="1"
                                    StretchDirection="Both"
                                    Stretch="Uniform">

                                    <Grid>

                                        <Canvas
                                            x:Name="statusTrendGrid20m"
                                            Width="1000"
                                            Height="12"/>

                                        <Rectangle
                                            Height="12"
                                            Stroke="Silver"
                                            StrokeThickness="0.5" />

                                    </Grid>

                                </Viewbox>

                            </Grid>

                        </Grid>

                        <Grid
                            Grid.Column="1"
                            Grid.Row="1"
                            Margin="16,0,0,0">

                            <Grid.ColumnDefinitions>
                                <ColumnDefinition
                                    Width="*" />
                                <ColumnDefinition
                                    Width="*" />
                                <ColumnDefinition
                                    Width="*" />
                                <ColumnDefinition
                                    Width="*" />
                                <ColumnDefinition
                                    Width="*" />
                                <ColumnDefinition
                                    Width="*" />
                                <ColumnDefinition
                                    Width="*" />
                                <ColumnDefinition
                                    Width="*" />
                            </Grid.ColumnDefinitions>

                            <Viewbox
                                Grid.Column="0"
                                Grid.ColumnSpan="2"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Left">

                                <Label
                                    Content="-20 mins" />

                            </Viewbox>

                            <Viewbox
                                Grid.Column="1"
                                Grid.ColumnSpan="2"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Center">

                                <Label
                                    Content="-15" />

                            </Viewbox>

                            <Viewbox
                                Grid.Column="3"
                                Grid.ColumnSpan="2"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Center">

                                <Label
                                    Content="-10" />

                            </Viewbox>

                            <Viewbox
                                Grid.Column="5"
                                Grid.ColumnSpan="2"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Center">

                                <Label
                                    Content="-5" />

                            </Viewbox>

                            <Viewbox
                                Grid.Column="10"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Right">

                                <Label
                                    Content="0" />

                            </Viewbox>

                        </Grid>

                        <Separator
                            Grid.Column="0"
                            Grid.Row="1"
                            Grid.ColumnSpan="3"
                            VerticalAlignment="Bottom" />

                        <!--PITCH-->
                        <Grid
                            Grid.Column="0"
                            Grid.Row="2">

                            <Grid.RowDefinitions>
                                <RowDefinition
                                    Height="4*" />
                                <RowDefinition
                                    Height="*" />
                                <RowDefinition
                                    Height="4*" />
                            </Grid.RowDefinitions>


                            <Viewbox
                                Grid.Row="1"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Center">

                                <TextBlock
                                    Text="Pitch" />

                            </Viewbox>

                        </Grid>

                        <telerik:RadCartesianChart
                            Grid.Column="1"
                            Grid.Row="2"
                            Name="chartPitch20m"
                            Margin="-10,32,0,0">

                            <telerik:LineSeries
                                ZIndex="200"
                                Stroke="{StaticResource ColorGraphLine}"
                                CategoryBinding="timestamp"
                                ValueBinding="graphData">

                                <telerik:LineSeries.RenderOptions>
                                    <telerik:XamlRenderOptions
                                        DefaultVisualsRenderMode="Batch" />
                                </telerik:LineSeries.RenderOptions>

                            </telerik:LineSeries>

                            <telerik:LineSeries
                                ZIndex="200"
                                Stroke="{StaticResource ColorGraphLine}"
                                CategoryBinding="timestamp"
                                ValueBinding="graphData">

                                <telerik:LineSeries.RenderOptions>
                                    <telerik:XamlRenderOptions
                                        DefaultVisualsRenderMode="Batch" />
                                </telerik:LineSeries.RenderOptions>

                            </telerik:LineSeries>

                            <telerik:RadCartesianChart.HorizontalAxis>
                                <telerik:DateTimeContinuousAxis
                                    x:Name="chartPitchHorizontalAxis20m"
                                    MajorStepUnit="Minute"
                                    MajorStep="5"
                                    ShowLabels="False"
                                    PlotMode="OnTicks"
                                    TickOrigin="{Binding alignmentTime}"
                                    Minimum="{Binding alignmentTimeMin20m}"
                                    Maximum="{Binding alignmentTimeMax}" />
                            </telerik:RadCartesianChart.HorizontalAxis>

                            <telerik:RadCartesianChart.VerticalAxis>
                                <telerik:LinearAxis
                                    x:Name="chartPitchVerticalAxis20m"
                                    Maximum="{Binding pitchChartAxisMax20m}"
                                    Minimum="{Binding pitchChartAxisMin20m}" >

                                    <telerik:LinearAxis.LabelTemplate>
                                        <DataTemplate>
                                            <TextBlock Text="{Binding Converter={StaticResource LabelAbsConverter}}" />
                                        </DataTemplate>
                                    </telerik:LinearAxis.LabelTemplate>

                                    <telerik:LinearAxis.TitleTemplate>
                                        <DataTemplate>
                                            <DockPanel>
                                                <Label
                                                Content="U"
                                                DockPanel.Dock="Right"
                                                Margin="150,0,0,0">
                                                    <Label.LayoutTransform>
                                                        <TransformGroup>
                                                            <RotateTransform
                                                            Angle="-90" />
                                                            <ScaleTransform
                                                            ScaleX="-1"
                                                            ScaleY="-1" />
                                                        </TransformGroup>
                                                    </Label.LayoutTransform>
                                                </Label>
                                                <Label
                                                Content="D"
                                                DockPanel.Dock="Left">
                                                    <Label.LayoutTransform>
                                                        <TransformGroup>
                                                            <RotateTransform
                                                            Angle="-90" />
                                                            <ScaleTransform
                                                            ScaleX="-1"
                                                            ScaleY="-1" />
                                                        </TransformGroup>
                                                    </Label.LayoutTransform>
                                                </Label>
                                            </DockPanel>
                                        </DataTemplate>
                                    </telerik:LinearAxis.TitleTemplate>

                                </telerik:LinearAxis>
                            </telerik:RadCartesianChart.VerticalAxis>

                            <telerik:RadCartesianChart.Grid>
                                <telerik:CartesianChartGrid
                                    MajorLinesVisibility="XY" />
                            </telerik:RadCartesianChart.Grid>

                            <telerik:RadCartesianChart.Annotations>

                                <telerik:CartesianGridLineAnnotation
                                    Axis="{Binding ElementName=chartPitchVerticalAxis20m}"
                                    Value="{Binding pitchChartAnnotationRedTopMin20m}"
                                    Stroke="{StaticResource ColorRed}"
                                    StrokeThickness="2" />

                                <telerik:CartesianGridLineAnnotation
                                    Axis="{Binding ElementName=chartPitchVerticalAxis20m}"
                                    Value="{Binding pitchChartAnnotationRedBottomMin20m}"
                                    Stroke="{StaticResource ColorRed}"
                                    StrokeThickness="2" />

                            </telerik:RadCartesianChart.Annotations>

                        </telerik:RadCartesianChart>

                        <Grid
                            Grid.Column="2"
                            Grid.Row="2"
                            Margin="8">

                            <Grid.RowDefinitions>
                                <RowDefinition
                                    Height="*" />
                                <RowDefinition
                                    Height="*" />
                                <RowDefinition
                                    Height="*" />
                                <RowDefinition
                                    Height="*" />
                                <RowDefinition
                                    Height="*" />
                                <RowDefinition
                                    Height="*" />
                                <RowDefinition
                                    Height="3*" />
                            </Grid.RowDefinitions>

                            <Grid
                                Grid.Row="1"
                                Name="gridMaxPitchUp20m">

                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition
                                        Width="4*" />
                                    <ColumnDefinition
                                        Width="2*" />
                                </Grid.ColumnDefinitions>

                                <Viewbox
                                    Grid.Column="0"
                                    StretchDirection="Both"
                                    Stretch="Uniform"
                                    HorizontalAlignment="Left">

                                    <TextBlock
                                        Text="Max Pitch" />

                                </Viewbox>

                                <Viewbox
                                    Grid.Column="1"
                                    StretchDirection="Both"
                                    Stretch="Uniform"
                                    HorizontalAlignment="Right">

                                    <TextBlock
                                        Text="{Binding Path=pitchMaxUp20mString2}"
                                        FontSize="{StaticResource fontSizeData}" />

                                </Viewbox>

                            </Grid>

                            <Grid
                                Grid.Row="2"
                                Name="gridMaxPitchDown20m">

                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition
                                        Width="4*" />
                                    <ColumnDefinition
                                        Width="2*" />
                                </Grid.ColumnDefinitions>

                                <Viewbox
                                    Grid.Column="1"
                                    StretchDirection="Both"
                                    Stretch="Uniform"
                                    HorizontalAlignment="Right">

                                    <TextBlock
                                        Text="{Binding Path=pitchMaxDown20mString2}"
                                        FontSize="{StaticResource fontSizeData}" />

                                </Viewbox>

                            </Grid>

                            <Grid
                                Grid.Row="4">

                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition
                                        Width="4*" />
                                    <ColumnDefinition
                                        Width="2*" />
                                </Grid.ColumnDefinitions>

                                <Viewbox
                                    Grid.Column="0"
                                    StretchDirection="Both"
                                    Stretch="Uniform"
                                    HorizontalAlignment="Left">

                                    <TextBlock
                                        Text="Limit" />

                                </Viewbox>

                                <Viewbox
                                    Grid.Column="1"
                                    StretchDirection="Both"
                                    Stretch="Uniform"
                                    HorizontalAlignment="Right">

                                    <TextBlock
                                        Text="{Binding Path=pitchRollLimitString}"
                                        FontSize="{StaticResource fontSizeData}" />

                                </Viewbox>

                            </Grid>

                        </Grid>

                        <!--Pitch X-axis labels-->
                        <Grid
                            Grid.Column="1"
                            Grid.Row="3"
                            Margin="20,0,0,0">

                            <Grid.ColumnDefinitions>
                                <ColumnDefinition
                                    Width="*" />
                                <ColumnDefinition
                                    Width="*" />
                                <ColumnDefinition
                                    Width="*" />
                                <ColumnDefinition
                                    Width="*" />
                                <ColumnDefinition
                                    Width="*" />
                                <ColumnDefinition
                                    Width="*" />
                                <ColumnDefinition
                                    Width="*" />
                                <ColumnDefinition
                                    Width="*" />
                            </Grid.ColumnDefinitions>

                            <Viewbox
                                Grid.Column="0"
                                Grid.ColumnSpan="2"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Left">

                                <Label
                                    Content="-20 mins" />

                            </Viewbox>

                            <Viewbox
                                Grid.Column="1"
                                Grid.ColumnSpan="2"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Center">

                                <Label
                                    Content="-15" />

                            </Viewbox>

                            <Viewbox
                                Grid.Column="3"
                                Grid.ColumnSpan="2"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Center">

                                <Label
                                    Content="-10" />

                            </Viewbox>

                            <Viewbox
                                Grid.Column="5"
                                Grid.ColumnSpan="2"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Center">

                                <Label
                                    Content="-5" />

                            </Viewbox>

                            <Viewbox
                                Grid.Column="10"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Right">

                                <Label
                                    Content="0" />

                            </Viewbox>

                        </Grid>

                        <Separator
                            Grid.Column="0"
                            Grid.Row="3"
                            Grid.ColumnSpan="3"
                            VerticalAlignment="Bottom"/>

                        <!--ROLL-->
                        <Grid
                            Grid.Column="0"
                            Grid.Row="4">

                            <Grid.RowDefinitions>
                                <RowDefinition
                                    Height="4*" />
                                <RowDefinition
                                    Height="*" />
                                <RowDefinition
                                    Height="4*" />
                            </Grid.RowDefinitions>


                            <Viewbox
                                Grid.Row="1"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Center">

                                <TextBlock
                                    Text="Roll" />

                            </Viewbox>

                        </Grid>

                        <telerik:RadCartesianChart
                            Grid.Column="1"
                            Grid.Row="4"
                            Name="chartRoll20m"
                            Margin="-10,32,0,0">

                            <telerik:LineSeries
                                ZIndex="200"
                                Stroke="{StaticResource ColorGraphLine}"
                                CategoryBinding="timestamp"
                                ValueBinding="graphData">

                                <telerik:LineSeries.RenderOptions>
                                    <telerik:XamlRenderOptions
                                        DefaultVisualsRenderMode="Batch" />
                                </telerik:LineSeries.RenderOptions>

                            </telerik:LineSeries>

                            <telerik:LineSeries
                                ZIndex="200"
                                Stroke="{StaticResource ColorGraphLine}"
                                CategoryBinding="timestamp"
                                ValueBinding="graphData">

                                <telerik:LineSeries.RenderOptions>
                                    <telerik:XamlRenderOptions
                                        DefaultVisualsRenderMode="Batch" />
                                </telerik:LineSeries.RenderOptions>

                            </telerik:LineSeries>

                            <telerik:RadCartesianChart.HorizontalAxis>
                                <telerik:DateTimeContinuousAxis
                                    MajorStepUnit="Minute"
                                    MajorStep="5"
                                    ShowLabels="False"
                                    PlotMode="OnTicks"
                                    TickOrigin="{Binding alignmentTime}"
                                    Minimum="{Binding alignmentTimeMin20m}"
                                    Maximum="{Binding alignmentTimeMax}" />
                            </telerik:RadCartesianChart.HorizontalAxis>

                            <telerik:RadCartesianChart.VerticalAxis>
                                <telerik:LinearAxis
                                    x:Name="chartRollVerticalAxis20m"
                                    Maximum="{Binding rollChartAxisMax20m}"
                                    Minimum="{Binding rollChartAxisMin20m}" >

                                    <telerik:LinearAxis.LabelTemplate>
                                        <DataTemplate>
                                            <TextBlock Text="{Binding Converter={StaticResource LabelAbsConverter}}" />
                                        </DataTemplate>
                                    </telerik:LinearAxis.LabelTemplate>

                                    <telerik:LinearAxis.TitleTemplate>
                                        <DataTemplate>
                                            <DockPanel>
                                                <Label
                                                Content="L"
                                                DockPanel.Dock="Right"
                                                Margin="150,0,0,0">
                                                    <Label.LayoutTransform>
                                                        <TransformGroup>
                                                            <RotateTransform
                                                            Angle="-90" />
                                                            <ScaleTransform
                                                            ScaleX="-1"
                                                            ScaleY="-1" />
                                                        </TransformGroup>
                                                    </Label.LayoutTransform>
                                                </Label>
                                                <Label
                                                Content="R"
                                                DockPanel.Dock="Left">
                                                    <Label.LayoutTransform>
                                                        <TransformGroup>
                                                            <RotateTransform
                                                            Angle="-90" />
                                                            <ScaleTransform
                                                            ScaleX="-1"
                                                            ScaleY="-1" />
                                                        </TransformGroup>
                                                    </Label.LayoutTransform>
                                                </Label>
                                            </DockPanel>
                                        </DataTemplate>
                                    </telerik:LinearAxis.TitleTemplate>

                                </telerik:LinearAxis>

                            </telerik:RadCartesianChart.VerticalAxis>

                            <telerik:RadCartesianChart.Grid>
                                <telerik:CartesianChartGrid
                                    MajorLinesVisibility="XY" />
                            </telerik:RadCartesianChart.Grid>

                            <telerik:RadCartesianChart.Annotations>

                                <telerik:CartesianGridLineAnnotation
                                    Axis="{Binding ElementName=chartRollVerticalAxis20m}"
                                    Value="{Binding rollChartAnnotationRedTopMin20m}"
                                    Stroke="{StaticResource ColorRed}"
                                    StrokeThickness="2" />

                                <telerik:CartesianGridLineAnnotation
                                    Axis="{Binding ElementName=chartRollVerticalAxis20m}"
                                    Value="{Binding rollChartAnnotationRedBottomMin20m}"
                                    Stroke="{StaticResource ColorRed}"
                                    StrokeThickness="2" />

                            </telerik:RadCartesianChart.Annotations>

                        </telerik:RadCartesianChart>

                        <Grid
                            Grid.Column="2"
                            Grid.Row="4"
                            Margin="8">

                            <Grid.RowDefinitions>
                                <RowDefinition
                                    Height="*" />
                                <RowDefinition
                                    Height="*" />
                                <RowDefinition
                                    Height="*" />
                                <RowDefinition
                                    Height="*" />
                                <RowDefinition
                                    Height="*" />
                                <RowDefinition
                                    Height="*" />
                                <RowDefinition
                                    Height="3*" />
                            </Grid.RowDefinitions>

                            <Grid
                                Grid.Row="1"
                                Name="gridMaxRollRight20m">

                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition
                                        Width="4*" />
                                    <ColumnDefinition
                                        Width="2*" />
                                </Grid.ColumnDefinitions>

                                <Viewbox
                                    Grid.Column="0"
                                    StretchDirection="Both"
                                    Stretch="Uniform"
                                    HorizontalAlignment="Left">

                                    <TextBlock
                                        Text="Max Roll" />

                                </Viewbox>

                                <Viewbox
                                    Grid.Column="1"
                                    StretchDirection="Both"
                                    Stretch="Uniform"
                                    HorizontalAlignment="Right">

                                    <TextBlock
                                        Text="{Binding Path=rollMaxLeft20mString2}"
                                        FontSize="{StaticResource fontSizeData}" />

                                </Viewbox>

                            </Grid>

                            <Grid
                                Grid.Row="2"
                                Name="gridMaxRollLeft20m">

                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition
                                        Width="4*" />
                                    <ColumnDefinition
                                        Width="2*" />
                                </Grid.ColumnDefinitions>

                                <Viewbox
                                    Grid.Column="1"
                                    StretchDirection="Both"
                                    Stretch="Uniform"
                                    HorizontalAlignment="Right">

                                    <TextBlock
                                        Text="{Binding Path=rollMaxRight20mString2}"
                                        FontSize="{StaticResource fontSizeData}" />

                                </Viewbox>

                            </Grid>

                            <Grid
                                Grid.Row="4">

                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition
                                        Width="4*" />
                                    <ColumnDefinition
                                        Width="2*" />
                                </Grid.ColumnDefinitions>

                                <Viewbox
                                    Grid.Column="0"
                                    StretchDirection="Both"
                                    Stretch="Uniform"
                                    HorizontalAlignment="Left">

                                    <TextBlock
                                        Text="Limit" />

                                </Viewbox>

                                <Viewbox
                                    Grid.Column="1"
                                    StretchDirection="Both"
                                    Stretch="Uniform"
                                    HorizontalAlignment="Right">

                                    <TextBlock
                                        Text="{Binding Path=pitchRollLimitString}"
                                        FontSize="{StaticResource fontSizeData}" />

                                </Viewbox>

                            </Grid>

                        </Grid>

                        <!--Roll X-axis labels-->
                        <Grid
                            Grid.Column="1"
                            Grid.Row="5"
                            Margin="20,0,0,0">

                            <Grid.ColumnDefinitions>
                                <ColumnDefinition
                                    Width="*" />
                                <ColumnDefinition
                                    Width="*" />
                                <ColumnDefinition
                                    Width="*" />
                                <ColumnDefinition
                                    Width="*" />
                                <ColumnDefinition
                                    Width="*" />
                                <ColumnDefinition
                                    Width="*" />
                                <ColumnDefinition
                                    Width="*" />
                                <ColumnDefinition
                                    Width="*" />
                            </Grid.ColumnDefinitions>

                            <Viewbox
                                Grid.Column="0"
                                Grid.ColumnSpan="2"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Left">

                                <Label
                                    Content="-20 mins" />

                            </Viewbox>

                            <Viewbox
                                Grid.Column="1"
                                Grid.ColumnSpan="2"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Center">

                                <Label
                                    Content="-15" />

                            </Viewbox>

                            <Viewbox
                                Grid.Column="3"
                                Grid.ColumnSpan="2"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Center">

                                <Label
                                    Content="-10" />

                            </Viewbox>

                            <Viewbox
                                Grid.Column="5"
                                Grid.ColumnSpan="2"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Center">

                                <Label
                                    Content="-5" />

                            </Viewbox>

                            <Viewbox
                                Grid.Column="10"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Right">

                                <Label
                                    Content="0" />

                            </Viewbox>

                        </Grid>

                        <Separator
                            Grid.Column="0"
                            Grid.Row="5"
                            Grid.ColumnSpan="4"
                            VerticalAlignment="Bottom" />

                        <!--INCLINATION-->
                        <Grid
                            Grid.Column="0"
                            Grid.Row="6">

                            <Grid.RowDefinitions>
                                <RowDefinition
                                    Height="4*" />
                                <RowDefinition
                                    Height="*" />
                                <RowDefinition
                                    Height="4*" />
                            </Grid.RowDefinitions>


                            <Viewbox
                                Grid.Row="1"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Center">

                                <TextBlock
                                    Text="Inclination" />

                            </Viewbox>

                        </Grid>

                        <telerik:RadCartesianChart
                            Grid.Column="1"
                            Grid.Row="6"
                            Name="chartInclination20m"
                            Margin="0,32,0,0">

                            <telerik:LineSeries
                                ZIndex="200"
                                Stroke="{StaticResource ColorGraphLine}"
                                CategoryBinding="timestamp"
                                ValueBinding="graphData">

                                <telerik:LineSeries.RenderOptions>
                                    <telerik:XamlRenderOptions
                                        DefaultVisualsRenderMode="Batch" />
                                </telerik:LineSeries.RenderOptions>

                            </telerik:LineSeries>

                            <telerik:RadCartesianChart.HorizontalAxis>
                                <telerik:DateTimeContinuousAxis
                                    MajorStepUnit="Minute"
                                    MajorStep="5"
                                    ShowLabels="False"
                                    PlotMode="OnTicks"
                                    TickOrigin="{Binding alignmentTime}"
                                    Minimum="{Binding alignmentTimeMin20m}"
                                    Maximum="{Binding alignmentTimeMax}" />
                            </telerik:RadCartesianChart.HorizontalAxis>

                            <telerik:RadCartesianChart.VerticalAxis>
                                <telerik:LinearAxis
                                    x:Name="chartInclinationVerticalAxis20m"
                                    Maximum="{Binding inclinationChartAxisMax20m}"
