<UserControl
    x:Class="HMS_Client.EMS"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:telerik="http://schemas.telerik.com/2008/xaml/presentation"
    xmlns:uc="clr-namespace:HMS_Client"
    mc:Ignorable="d"
    d:DesignHeight="1100"
    d:DesignWidth="1900">

    <Grid>

        <Grid.ColumnDefinitions>
            <ColumnDefinition
                Width="24*" />
            <ColumnDefinition
                Width="17*" />
        </Grid.ColumnDefinitions>


        <Grid
            Grid.Column="2">

            <Grid.RowDefinitions>
                <RowDefinition
                    Height="10*" />
                <RowDefinition
                    Height="6*" />
            </Grid.RowDefinitions>

            <!--Wind & Heading-->
            <uc:WindHeading
                Grid.Row="0"
                x:Name="ucWindHeading" />

            <!--Meteorological-->
            <uc:NOROG_Meteorological
                Grid.Row="1"
                x:Name="ucMeteorological" />

        </Grid>

    </Grid>

</UserControl>
