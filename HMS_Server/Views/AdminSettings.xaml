<UserControl
    x:Class="HMS_Server.AdminSettings"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:telerik="http://schemas.telerik.com/2008/xaml/presentation"
    mc:Ignorable="d"
    d:DesignHeight="1000"
    d:DesignWidth="1500">

    <telerik:RadTabControl
        DockPanel.Dock="Top"
        Name="tcMainMenu"
        Margin="8"
        TabStripPlacement="Top"
        TabOrientation="Horizontal"
        HeaderBackground="{StaticResource ColorHorizontalBarBackground}">

        <!--==============================================================-->
        <!--Side Menu: Input-->
        <!--==============================================================-->
        <telerik:RadTabItem
            Header="General">

            <Grid>

                <Grid.ColumnDefinitions>
                    <ColumnDefinition
                        Width="*" />
                    <ColumnDefinition
                        Width="*" />
                    <ColumnDefinition
                        Width="*" />
                </Grid.ColumnDefinitions>

                <Grid.RowDefinitions>
                    <RowDefinition
                        Height="*" />
                    <RowDefinition
                        Height="*" />
                    <RowDefinition
                        Height="*" />
                </Grid.RowDefinitions>

                <telerik:GroupBox
                    Grid.Row="0"
                    Grid.Column="0"
                    Header="HMS"
                    Margin="8">

                    <Grid
                        Margin="8">

                        <Grid.RowDefinitions>
                            <RowDefinition
                                Height="*" />
                            <RowDefinition
                                Height="*" />
                            <RowDefinition
                                Height="*" />
                            <RowDefinition
                                Height="*" />
                            <RowDefinition
                                Height="*" />
                            <RowDefinition
                                Height="*" />
                            <RowDefinition
                                Height="*" />
                        </Grid.RowDefinitions>

                        <!--Regulation Standard-->
                        <Grid
                            Grid.Row="0">

                            <Grid.ColumnDefinitions>
                                <ColumnDefinition
                                    Width="*" />
                                <ColumnDefinition
                                    Width="*" />
                            </Grid.ColumnDefinitions>

                            <Viewbox
                                Grid.Column="0"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Left">

                                <telerik:Label
                                    Content="Regulation Standard"
                                    VerticalAlignment="Center" />
                            </Viewbox>

                            <Viewbox
                                Grid.Column="1"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Right">

                                <telerik:RadComboBox
                                    Name="cboRegulationStandard"
                                    Width="200"
                                    SelectionChanged="cboRegulationStandard_SelectionChanged">
                                </telerik:RadComboBox>
                            </Viewbox>

                        </Grid>

                        <!--Vessel Name-->
                        <Grid
                            Grid.Row="1">

                            <Grid.ColumnDefinitions>
                                <ColumnDefinition
                                    Width="*" />
                                <ColumnDefinition
                                    Width="*" />
                            </Grid.ColumnDefinitions>

                            <Viewbox
                                Grid.Column="0"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Left">

                                <telerik:Label
                                    Content="Vessel Name"
                                    VerticalAlignment="Center" />
                            </Viewbox>

                            <Viewbox
                                Grid.Column="1"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Right">

                                <TextBox
                                    Name="tbVesselName"
                                    HorizontalAlignment="Right"
                                    Width="200"
                                    Text="{Binding Path=vesselName}"
                                    LostFocus="tbVesselName_LostFocus"
                                    KeyDown="tbVesselName_KeyDown" />
                            </Viewbox>
                        </Grid>

                        <!--Vessel Image-->
                        <Grid
                            Grid.Row="2">

                            <Grid.ColumnDefinitions>
                                <ColumnDefinition
                                    Width="*" />
                                <ColumnDefinition
                                    Width="*" />
                            </Grid.ColumnDefinitions>

                            <Viewbox
                                Grid.Column="0"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Left">

                                <telerik:Label
                                    Width="200"
                                    Content="Vessel Image"
                                    VerticalAlignment="Center" />

                            </Viewbox>

                            <Viewbox
                                Grid.Column="1"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Right">

                                <telerik:RadComboBox
                                    Name="cboVisVesselImage"
                                    SelectionChanged="cboVisVesselImage_SelectionChanged" />

                            </Viewbox>

                        </Grid>

                        <!--Helideck Lights Output-->
                        <Grid
                            Grid.Row="3">

                            <Grid.ColumnDefinitions>
                                <ColumnDefinition
                                    Width="*" />
                                <ColumnDefinition
                                    Width="*" />
                            </Grid.ColumnDefinitions>

                            <Viewbox
                                Grid.Column="0"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Left">

                                <telerik:Label
                                    Content="Helideck Lights Output"
                                    VerticalAlignment="Center" />
                            </Viewbox>


                            <Viewbox
                                Grid.Column="1"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Right">

                                <CheckBox
                                    IsChecked="{Binding Path=helideckLightsOutput, Mode=TwoWay}"
                                    VerticalAlignment="Center"
                                    HorizontalAlignment="Right" />
                            </Viewbox>
                        </Grid>

                        <!--EMS Page-->
                        <Grid
                            Grid.Row="4">

                            <Grid.ColumnDefinitions>
                                <ColumnDefinition
                                    Width="*" />
                                <ColumnDefinition
                                    Width="*" />
                            </Grid.ColumnDefinitions>

                            <Viewbox
                                Grid.Column="0"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Left">

                                <telerik:Label
                                    Content="EMS Page"
                                    VerticalAlignment="Center" />
                            </Viewbox>

                            <Viewbox
                                Grid.Column="1"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Right">

                                <CheckBox
                                    Name="chkEnableEMS"
                                    IsChecked="{Binding Path=enableEMS, Mode=TwoWay}"
                                    VerticalAlignment="Center"
                                    HorizontalAlignment="Right"
                                    Click="chkEnableEMS_Click" />
                            </Viewbox>

                        </Grid>

                        <!--Auto Start HMS Processing-->
                        <Grid
                            Grid.Row="5">

                            <Grid.ColumnDefinitions>
                                <ColumnDefinition
                                    Width="*" />
                                <ColumnDefinition
                                    Width="*" />
                            </Grid.ColumnDefinitions>

                            <Viewbox
                                Grid.Column="0"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Left">

                                <telerik:Label
                                    Content="Auto Start HMS Processing"
                                    VerticalAlignment="Center" />
                            </Viewbox>

                            <Viewbox
                                Grid.Column="1"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Right">

                                <CheckBox
                                    Name="chkAutoStartHMS"
                                    IsChecked="{Binding Path=autoStartHMS, Mode=TwoWay}"
                                    VerticalAlignment="Center"
                                    HorizontalAlignment="Right" />
                            </Viewbox>

                        </Grid>

                    </Grid>

                </telerik:GroupBox>

                <!--HEADING-->
                <telerik:GroupBox
                    Grid.Column="0"
                    Grid.Row="1"
                    Header="HEADING"
                    Margin="8">

                    <Grid
                        Margin="8">

                        <Grid.RowDefinitions>
                            <RowDefinition
                                Height="*" />
                            <RowDefinition
                                Height="*" />
                            <RowDefinition
                                Height="*" />
                            <RowDefinition
                                Height="*" />
                            <RowDefinition
                                Height="*" />
                            <RowDefinition
                                Height="*" />
                            <RowDefinition
                                Height="*" />
                        </Grid.RowDefinitions>

                        <!--Vessel Heading Reference-->
                        <Grid
                            Grid.Row="0">

                            <Grid.ColumnDefinitions>
                                <ColumnDefinition
                                    Width="*" />
                                <ColumnDefinition
                                    Width="*" />
                            </Grid.ColumnDefinitions>

                            <Viewbox
                                Grid.Column="0"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Left">

                                <telerik:Label
                                    Name="lbVesselHdgRef"
                                    Content="Heading Reference"
                                    VerticalAlignment="Center" />
                            </Viewbox>

                            <Viewbox
                                Grid.Column="1"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Right">

                                <telerik:RadComboBox
                                    Name="cboVesselHdgRef"
                                    Width="200"
                                    HorizontalAlignment="Right"
                                    SelectionChanged="cboVesselHdgRef_SelectionChanged">
                                </telerik:RadComboBox>
                            </Viewbox>
                        </Grid>

                        <!--Magnetic Declination-->
                        <Grid
                            Grid.Row="1">

                            <Grid.ColumnDefinitions>
                                <ColumnDefinition
                                    Width="*" />
                                <ColumnDefinition
                                    Width="*" />
                            </Grid.ColumnDefinitions>

                            <Viewbox
                                Grid.Column="0"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Left">

                                <telerik:Label
                                    Name="lbMagneticDeclination"
                                    Content="Magnetic Declination"
                                    VerticalAlignment="Center" />
                            </Viewbox>

                            <Viewbox
                                Grid.Column="1"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Right">

                                <TextBox
                                    Name="tbMagneticDeclination"
                                    Width="200"
                                    HorizontalAlignment="Right"
                                    Text="{Binding Path=magneticDeclination}"
                                    LostFocus="tbMagneticDeclination_LostFocus"
                                    KeyDown="tbMagneticDeclination_KeyDown" />
                            </Viewbox>
                        </Grid>

                        <!--Helideck Heading Offset-->
                        <Grid
                            Grid.Row="2">

                            <Grid.ColumnDefinitions>
                                <ColumnDefinition
                                    Width="*" />
                                <ColumnDefinition
                                    Width="*" />
                            </Grid.ColumnDefinitions>

                            <Viewbox
                                Grid.Column="0"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Left">

                                <telerik:Label
                                    Content="Helideck Heading Offset"
                                    VerticalAlignment="Center" />
                            </Viewbox>

                            <Viewbox
                                Grid.Column="1"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Right">

                                <TextBox
                                    Name="tbHelideckHeadingOffset"
                                    Width="200"
                                    HorizontalAlignment="Right"
                                    Text="{Binding Path=helideckHeadingOffset}"
                                    LostFocus="tbHelideckHeadingOffset_LostFocus"
                                    KeyDown="tbHelideckHeadingOffset_KeyDown" />
                            </Viewbox>
                        </Grid>

                    </Grid>

                </telerik:GroupBox>

                <!--SERVER-->
                <telerik:GroupBox
                    Grid.Row="0"
                    Grid.Column="1"
                    Header="SERVER"
                    Margin="8">

                    <Grid
                        Margin="8">

                        <Grid.RowDefinitions>
                            <RowDefinition
                                Height="*" />
                            <RowDefinition
                                Height="*" />
                            <RowDefinition
                                Height="*" />
                            <RowDefinition
                                Height="*" />
                            <RowDefinition
                                Height="*" />
                            <RowDefinition
                                Height="*" />
                            <RowDefinition
                                Height="*" />
                        </Grid.RowDefinitions>

                        <!--Address-->
                        <Grid
                            Grid.Row="0">

                            <Grid.ColumnDefinitions>
                                <ColumnDefinition
                                    Width="*" />
                                <ColumnDefinition
                                    Width="*" />
                            </Grid.ColumnDefinitions>

                            <Viewbox
                                Grid.Column="0"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Left">

                                <telerik:Label
                                    Content="Address"
                                    VerticalAlignment="Center" />
                            </Viewbox>

                            <Viewbox
                                Grid.Column="1"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Right">

                                <telerik:Label
                                    Name="lbSettingsServerAddress"
                                    Width="110"
                                    HorizontalAlignment="Right" />
                            </Viewbox>
                        </Grid>

                        <!--Port-->
                        <Grid
                            Grid.Row="1">

                            <Grid.ColumnDefinitions>
                                <ColumnDefinition
                                    Width="*" />
                                <ColumnDefinition
                                    Width="*" />
                            </Grid.ColumnDefinitions>

                            <Viewbox
                                Grid.Column="0"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Left">

                                <telerik:Label
                                    Content="Port"
                                    VerticalAlignment="Center" />
                            </Viewbox>

                            <Viewbox
                                Grid.Column="1"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Right">

                                <TextBox
                                    Name="tbSettingsServerPort"
                                    Width="110"
                                    HorizontalAlignment="Right"
                                    Text="{Binding Path=serverPort}"
                                    LostFocus="tbSettingsServerPort_LostFocus"
                                    KeyDown="tbSettingsServerPort_KeyDown" />
                            </Viewbox>
                        </Grid>

                    </Grid>

                </telerik:GroupBox>

                <!--DATABASE-->
                <telerik:GroupBox
                    Grid.Row="0"
                    Grid.Column="2"
                    Grid.RowSpan="2"
                    Header="DATABASE"
                    Margin="8">

                    <Grid
                        Margin="8">

                        <Grid.RowDefinitions>
                            <RowDefinition
                                Height="*" />
                            <RowDefinition
                                Height="*" />
                            <RowDefinition
                                Height="*" />
                            <RowDefinition
                                Height="*" />
                            <RowDefinition
                                Height="*" />
                            <RowDefinition
                                Height="*" />
                            <RowDefinition
                                Height="*" />
                            <RowDefinition
                                Height="*" />
                            <RowDefinition
                                Height="*" />
                            <RowDefinition
                                Height="*" />
                            <RowDefinition
                                Height="*" />
                            <RowDefinition
                                Height="*" />
                            <RowDefinition
                                Height="*" />
                            <RowDefinition
                                Height="*" />
                            <RowDefinition
                                Height="*" />
                            <RowDefinition
                                Height="*" />
                        </Grid.RowDefinitions>

                        <!--Address-->
                        <Grid
                            Grid.Row="0">

                            <Grid.ColumnDefinitions>
                                <ColumnDefinition
                                    Width="*" />
                                <ColumnDefinition
                                    Width="*" />
                            </Grid.ColumnDefinitions>

                            <Viewbox
                                Grid.Column="0"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Left">

                                <telerik:Label
                                    Content="Address"
                                    VerticalAlignment="Center" />
                            </Viewbox>

                            <Viewbox
                                Grid.Column="1"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Right">

                                <TextBox
                                    Name="tbSettingsDatabaseAddress"
                                    Width="110"
                                    HorizontalContentAlignment="Left"
                                    HorizontalAlignment="Right"
                                    Text="{Binding Path=databaseAddress}"
                                    LostFocus="tbSettingsDatabaseAddress_LostFocus"
                                    KeyDown="tbSettingsDatabaseAddress_KeyDown" />
                            </Viewbox>
                        </Grid>

                        <!--Port-->
                        <Grid
                            Grid.Row="1">

                            <Grid.ColumnDefinitions>
                                <ColumnDefinition
                                    Width="*" />
                                <ColumnDefinition
                                    Width="*" />
                            </Grid.ColumnDefinitions>

                            <Viewbox
                                Grid.Column="0"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Left">

                                <telerik:Label
                                    Content="Port"
                                    VerticalAlignment="Center" />
                            </Viewbox>

                            <Viewbox
                                Grid.Column="1"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Right">

                                <TextBox
                                    Name="tbSettingsDatabasePort"
                                    Width="110"
                                    HorizontalAlignment="Right"
                                    HorizontalContentAlignment="Left"
                                    Text="{Binding Path=databasePort}"
                                    LostFocus="tbSettingsDatabasePort_LostFocus"
                                    KeyDown="tbSettingsDatabasePort_KeyDown" />
                            </Viewbox>
                        </Grid>

                        <!--Name-->
                        <Grid
                            Grid.Row="2">

                            <Grid.ColumnDefinitions>
                                <ColumnDefinition
                                    Width="*" />
                                <ColumnDefinition
                                    Width="*" />
                            </Grid.ColumnDefinitions>

                            <Viewbox
                                Grid.Column="0"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Left">

                                <telerik:Label
                                    Content="Name"
                                    VerticalAlignment="Center" />
                            </Viewbox>

                            <Viewbox
                                Grid.Column="1"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Right">

                                <TextBox
                                    Name="tbSettingsDatabaseName"
                                    Width="110"
                                    HorizontalAlignment="Right"
                                    Text="{Binding Path=databaseName}"
                                    LostFocus="tbSettingsDatabaseName_LostFocus"
                                    KeyDown="tbSettingsDatabaseName_KeyDown" />
                            </Viewbox>
                        </Grid>

                        <!--User ID-->
                        <Grid
                            Grid.Row="3">

                            <Grid.ColumnDefinitions>
                                <ColumnDefinition
                                    Width="*" />
                                <ColumnDefinition
                                    Width="*" />
                            </Grid.ColumnDefinitions>

                            <Viewbox
                                Grid.Column="0"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Left">

                                <telerik:Label
                                    Content="User ID"
                                    VerticalAlignment="Center" />
                            </Viewbox>

                            <Viewbox
                                Grid.Column="1"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Right">

                                <TextBox
                                    Name="tbSettingsDatabaseUserID"
                                    Width="110"
                                    HorizontalAlignment="Right"
                                    LostFocus="tbSettingsDatabaseUserID_LostFocus"
                                    KeyDown="tbSettingsDatabaseUserID_KeyDown" />
                            </Viewbox>
                        </Grid>

                        <!--Password-->
                        <Grid
                            Grid.Row="4">

                            <Grid.ColumnDefinitions>
                                <ColumnDefinition
                                    Width="*" />
                                <ColumnDefinition
                                    Width="*" />
                            </Grid.ColumnDefinitions>

                            <Viewbox
                                Grid.Column="0"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Left">

                                <telerik:Label
                                    Content="Password"
                                    VerticalAlignment="Center" />
                            </Viewbox>

                            <Viewbox
                                Grid.Column="1"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Right">

                                <TextBox
                                    Name="tbSettingsDatabasePassword"
                                    Width="110"
                                    HorizontalAlignment="Right"
                                    LostFocus="tbSettingsDatabasePassword_LostFocus"
                                    KeyDown="tbSettingsDatabasePassword_KeyDown" />
                            </Viewbox>
                        </Grid>

                        <!--Data Storage Time-->
                        <Grid
                            Grid.Row="5">

                            <Grid.ColumnDefinitions>
                                <ColumnDefinition
                                    Width="*" />
                                <ColumnDefinition
                                    Width="*" />
                            </Grid.ColumnDefinitions>

                            <Viewbox
                                Grid.Column="0"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Left">

                                <telerik:Label
                                    Content="Data Storage Time"
                                    VerticalAlignment="Center" />
                            </Viewbox>

                            <Viewbox
                                Grid.Column="1"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Right">

                                <TextBox
                                    Name="tbSettingsDatabaseDataStorageTime"
                                    Width="110"
                                    HorizontalAlignment="Right"
                                    Text="{Binding Path=databaseDataStorageTime}"
                                    LostFocus="tbSettingsDatabaseDataStorageTime_LostFocus"
                                    KeyDown="tbSettingsDatabaseDataStorageTime_KeyDown" />
                            </Viewbox>
                        </Grid>

                        <!--Messages Storage Time-->
                        <Grid
                            Grid.Row="6">

                            <Grid.ColumnDefinitions>
                                <ColumnDefinition
                                    Width="*" />
                                <ColumnDefinition
                                    Width="*" />
                            </Grid.ColumnDefinitions>

                            <Viewbox
                                Grid.Column="0"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Left">

                                <telerik:Label
                                    Content="Messages Storage Time"
                                    VerticalAlignment="Center" />
                            </Viewbox>

                            <Viewbox
                                Grid.Column="1"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Right">

                                <TextBox
                                    Name="tbSettingsDatabaseErrorMessagesStorageTime"
                                    Width="110"
                                    HorizontalAlignment="Right"
                                    Text="{Binding Path=databaseErrorMessageStorageTime}"
                                    LostFocus="tbSettingsDatabaseErrorMessagesStorageTime_LostFocus"
                                    KeyDown="tbSettingsDatabaseErrorMessagesStorageTime_KeyDown" />
                            </Viewbox>
                        </Grid>

                        <!--Remove Unused Database Tables-->
                        <Grid
                            Grid.Row="7">

                            <Grid.ColumnDefinitions>
                                <ColumnDefinition
                                    Width="3*" />
                                <ColumnDefinition
                                    Width="2*" />
                            </Grid.ColumnDefinitions>

                            <Viewbox
                                Grid.Column="0"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Left">

                                <telerik:Label
                                    Width="240"
                                    Content="Remove Unused Database Tables"
                                    VerticalAlignment="Center" />
                            </Viewbox>

                            <Viewbox
                                Grid.Column="1"
                                Margin="4"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Right">

                                <Button
                                    Name="btnRemoveUnusedDatabaseTables"
                                    Click="btnRemoveUnusedDatabaseTables_Click"
                                    HorizontalAlignment="Right"
                                    Width="110">Cleanup</Button>
                            </Viewbox>
                        </Grid>

                        <!--Empty All Database Tables-->
                        <Grid
                            Grid.Row="8">

                            <Grid.ColumnDefinitions>
                                <ColumnDefinition
                                    Width="3*" />
                                <ColumnDefinition
                                    Width="2*" />
                            </Grid.ColumnDefinitions>

                            <Viewbox
                                Grid.Column="0"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Left">

                                <telerik:Label
                                    Width="240"
                                    Content="Empty All Database Tables"
                                    VerticalAlignment="Center" />
                            </Viewbox>

                            <Viewbox
                                Grid.Column="1"
                                Margin="4"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Right">

                                <Button
                                    Name="btnEmptyDatabaseTables"
                                    Click="btnEmptyDatabaseTables_Click"
                                    HorizontalAlignment="Right"
                                    Width="110">Delete Data</Button>
                            </Viewbox>
                        </Grid>

                    </Grid>

                </telerik:GroupBox>

                <!--TIMING-->
                <telerik:GroupBox
                    Grid.Row="2"
                    Grid.Column="1"
                    Margin="8"
                    Header="TIMING">

                    <Grid
                        Margin="8">

                        <Grid.RowDefinitions>
                            <RowDefinition
                                Height="*" />
                            <RowDefinition
                                Height="*" />
                            <RowDefinition
                                Height="*" />
                            <RowDefinition
                                Height="*" />
                            <RowDefinition
                                Height="*" />
                            <RowDefinition
                                Height="*" />
                            <RowDefinition
                                Height="*" />
                        </Grid.RowDefinitions>

                        <!--Setup UI Data Limit-->
                        <Grid
                            Grid.Row="0">

                            <Grid.ColumnDefinitions>
                                <ColumnDefinition
                                    Width="*" />
                                <ColumnDefinition
                                    Width="*" />
                            </Grid.ColumnDefinitions>

                            <Viewbox
                                Grid.Column="0"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Left">

                                <telerik:Label
                                    Content="Setup UI Data Limit (ms)"
                                    VerticalAlignment="Center" />
                            </Viewbox>

                            <Viewbox
                                Grid.Column="1"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Right">

                                <TextBox
                                    Name="tbSettingsGUIDataLimit"
                                    Width="110"
                                    HorizontalAlignment="Right"
                                    Text="{Binding Path=setupGUIDataLimit}"
                                    LostFocus="tbSettingsGUIDataLimit_LostFocus"
                                    KeyDown="tbSettingsGUIDataLimit_KeyDown" />
                            </Viewbox>
                        </Grid>

                        <!--Server UI Update Frequency-->
                        <Grid
                            Grid.Row="1">

                            <Grid.ColumnDefinitions>
                                <ColumnDefinition
                                    Width="*" />
                                <ColumnDefinition
                                    Width="*" />
                            </Grid.ColumnDefinitions>

                            <Viewbox
                                Grid.Column="0"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Left">

                                <telerik:Label
                                    Content="Server UI Update Frequency"
                                    VerticalAlignment="Center" />
                            </Viewbox>

                            <Viewbox
                                Grid.Column="1"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Right">

                                <TextBox
                                    Name="tbSettingsServerUIFrequency"
                                    Width="110"
                                    HorizontalAlignment="Right"
                                    Text="{Binding Path=serverUIUpdateFrequency}"
                                    LostFocus="tbSettingsServerUIFrequency_LostFocus"
                                    KeyDown="tbSettingsServerUIFrequency_KeyDown" />
                            </Viewbox>
                        </Grid>

                        <!--Database Save Frequency-->
                        <Grid
                            Grid.Row="2">

                            <Grid.ColumnDefinitions>
                                <ColumnDefinition
                                    Width="*" />
                                <ColumnDefinition
                                    Width="*" />
                            </Grid.ColumnDefinitions>

                            <Viewbox
                                Grid.Column="0"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Left">

                                <telerik:Label
                                    Content="Database Save Frequency"
                                    VerticalAlignment="Center" />
                            </Viewbox>

                            <Viewbox
                                Grid.Column="1"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                HorizontalAlignment="Right">

                                <TextBox
                                    Name="tbSettingsDatabaseSaveFrequency"
                                    Width="110"
                                    HorizontalAlignment="Right"
