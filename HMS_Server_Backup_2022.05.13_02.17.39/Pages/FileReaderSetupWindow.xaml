<telerik:RadWindow
    x:Class="HMS_Server.FileReaderSetupWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:local="clr-namespace:HMS_Server"
    xmlns:telerik="http://schemas.telerik.com/2008/xaml/presentation"
    mc:Ignorable="d"
    telerik:Header="File Reader Data Setup"
    WindowState="Maximized"
    Closed="RadWindow_Closed"
    Height="1010"
    Width="1300">

    <telerik:RadWindow.Resources>
        <ResourceDictionary>
            <Style
                TargetType="local:FileReaderSetupWindow"
                BasedOn="{StaticResource RadWindowStyle}" />
        </ResourceDictionary>

    </telerik:RadWindow.Resources>

    <telerik:RadWindow.Icon>
        <Image
            Height="32"
            Source="../Icons/swire_flag.ico" />
    </telerik:RadWindow.Icon>

    <DockPanel>

        <!--Status Bar-->
        <StatusBar
            DockPanel.Dock="Bottom">
            <StatusBarItem>
                <TextBlock
                    Name="fileReadingStatus" />
            </StatusBarItem>
        </StatusBar>

        <Grid
            DockPanel.Dock="Top">
            <Grid.ColumnDefinitions>
                <ColumnDefinition
                    Width="400" />
                <ColumnDefinition
                    Width="*" />
            </Grid.ColumnDefinitions>

            <StackPanel
                Grid.Row="0"
                Grid.Column="0">

                <!--Basic Information-->
                <telerik:GroupBox
                    Header="Basic Information"
                    Margin="8">

                    <StackPanel>

                        <DockPanel>
                            <Label
                                Width="110"
                                Content="ID" />
                            <Label
                                Name="lbSensorID" />
                        </DockPanel>

                        <DockPanel>
                            <Label
                                Width="110"
                                Content="Name"
                                VerticalAlignment="Center" />
                            <Label
                                Name="lbSensorName" />
                        </DockPanel>

                        <DockPanel>
                            <Label
                                Width="110"
                                Content="Type"
                                VerticalAlignment="Center" />
                            <Label
                                Name="lbSensorType" />
                        </DockPanel>


                    </StackPanel>

                </telerik:GroupBox>

                <!--Settings-->
                <telerik:GroupBox
                    Header="Settings"
                    Margin="8">

                    <StackPanel>

                        <DockPanel>

                            <Label
                                Width="100"
                                Content="File Folder"
                                VerticalAlignment="Center" />

                            <TextBox
                                Name="tbFileFolder"
                                Text="{Binding fileFolder}"/>

                        </DockPanel>

                        <DockPanel>

                            <Label
                                Width="100"
                                Content="File Name"
                                VerticalAlignment="Center" />

                            <TextBox
                                Name="tbFileName"
                                Text="{Binding fileName}" />

                        </DockPanel>

                        <Button
                            Name="btnFilePath"
                            Click="btnFilePath_Click"
                            Width="110"
                            Margin="8"
                            HorizontalAlignment="Right">Set File Path</Button>

                        <DockPanel>
                            
                            <Label
                                Content="Read Frequency"
                                Width="200"                                
                                VerticalAlignment="Center" />
                            
                            <TextBox
                                Name="tbFileReadFrequency"
                                Text="{Binding Path=readFrequency}"
                                LostFocus="tbFileReadFrequency_LostFocus"
                                KeyDown="tbFileReadFrequency_KeyDown" />
                            
                        </DockPanel>

                    </StackPanel>

                </telerik:GroupBox>

                <!--Read Controls-->
                <telerik:GroupBox
                    Header="Read Controls"
                    Margin="8">

                    <StackPanel>

                        <StackPanel
                            Orientation="Horizontal"
                            HorizontalAlignment="Center">

                            <Button
                                x:Name="btnFileReaderStart"
                                Click="btnFileReaderStart_Click"
                                Width="120"
                                Margin="8"
                                Content="Start" />
                            <Button
                                x:Name="btnFileReaderStop"
                                Click="btnFileReaderStop_Click"
                                Width="120"
                                Margin="8"
                                IsEnabled="False"
                                Content="Stop" />

                        </StackPanel>

                        <Rectangle
                            Width="auto"
                            Height="7"
                            Margin="8">
                            <Rectangle.Fill>
                                <VisualBrush
                                    Stretch="None">
                                    <VisualBrush.Visual>
                                        <Border
                                            x:Name="bFileReaderStatus"
                                            CornerRadius="3,3,3,3"
                                            Width="120"
                                            Height="7"
                                            Background="{StaticResource ColorRed}"
                                            BorderThickness="1"
                                            BorderBrush="Gray" />
                                    </VisualBrush.Visual>
                                </VisualBrush>
                            </Rectangle.Fill>
                        </Rectangle>

                    </StackPanel>

                </telerik:GroupBox>

                <!--Data Display-->
                <telerik:GroupBox
                    Header="Data Display"
                    Margin="8">

                    <StackPanel>

                        <DockPanel>

                            <Label
                                Width="200"
                                Content="Data Lines"
                                VerticalAlignment="Center" />

                            <TextBox
                                Name="tbTotalDataLines"
                                Text="{Binding totalDataLinesString}"
                                LostFocus="tbTotalDataLines_LostFocus"
                                KeyDown="tbTotalDataLines_KeyDown" />

                        </DockPanel>

                        <Button
                            x:Name="chkClearAllData"
                            Click="chkClearAllData_Click"
                            HorizontalAlignment="Left"
                            Content="Clear All" />

                    </StackPanel>

                </telerik:GroupBox>

            </StackPanel>

            <telerik:RadTabControl
                Grid.Row="0"
                Grid.Column="1"
                Margin="8"
                HeaderBackground="{StaticResource ColorHorizontalBarBackground}">

                <!--1. Data Selection-->
                <telerik:RadTabItem
                    Name="tabDataSelection"
                    Header="1: Data Selection">

                    <Grid>

                        <Grid.ColumnDefinitions>
                            <ColumnDefinition
                                Width="407*" />
