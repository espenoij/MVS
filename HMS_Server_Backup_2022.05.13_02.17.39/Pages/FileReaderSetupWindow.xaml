                            </DockPanel>

                        </telerik:GroupBox>

                        <!--Selected Data-->
                        <telerik:GroupBox
                            Grid.Column="0"
                            Grid.Row="2"
                            Header="Selected Data"
                            Margin="8"
                            Grid.ColumnSpan="2">

                            <DockPanel>

                                <telerik:RadGridView
                                    Name="lvSelectedData"
                                    IsReadOnly="True"
                                    IsManipulationEnabled="False"
                                    CanUserSortColumns="False"
                                    ShowGroupPanel="False"
                                    ShowColumnHeaders="False"
                                    RowHeight="25"
                                    Margin="8"
                                    CanUserReorderColumns="False"
                                    CanUserDeleteRows="False"
                                    RowIndicatorVisibility="Collapsed"
                                    AutoGenerateColumns="False">

                                    <telerik:RadGridView.Columns>
                                        <telerik:GridViewDataColumn
                                            DataMemberBinding="{Binding selectedDataFieldString}" />
                                    </telerik:RadGridView.Columns>

                                </telerik:RadGridView>

                            </DockPanel>

                        </telerik:GroupBox>

                        <telerik:GroupBox
                            Grid.Column="2"
                            Grid.Row="0"
                            Header="Data Field Split"
                            Margin="8">

                            <StackPanel
                                Margin="8">

                                <DockPanel>
                                    <Label
                                        Width="120"
                                        Content="Delimiter"
                                        VerticalAlignment="Center" />
                                    <TextBox
                                        Name="tbFileReaderDelimiter"
                                        LostFocus="tbFileReaderDelimiter_LostFocus"
                                        KeyDown="tbFileReaderDelimiter_KeyDown" />
                                </DockPanel>

                                <Separator
                                    Margin="0,8" />

                                <CheckBox
                                    Name="cboFileReaderFixedPositionData"
                                    Content="Fixed Position Data"
                                    Checked="cboFixedPositionData_Checked"
                                    Unchecked="cboFixedPositionData_Unchecked" />

                                <DockPanel>
                                    <Label
                                        Width="120"
                                        Content="Start"
                                        VerticalAlignment="Center" />
                                    <TextBox
                                        Name="tbFileReaderFixedPositionDataStart"
                                        LostFocus="tbFixedPositionDataStart_LostFocus"
                                        KeyDown="tbFixedPositionDataStart_KeyDown"/>
                                </DockPanel>

                                <DockPanel>
                                    <Label
                                        Width="120"
                                        Content="Total"
                                        VerticalAlignment="Center" />
                                    <TextBox
                                        Name="tbFileReaderFixedPositionDataTotal"
                                        LostFocus="tbFixedPositionDataTotal_LostFocus"
                                        KeyDown="tbFixedPositionDataTotal_KeyDown"/>
                                </DockPanel>

