<UserControl
    x:Class="MVS.DataAnalysis"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:telerik="http://schemas.telerik.com/2008/xaml/presentation"
    xmlns:sys="clr-namespace:System;assembly=mscorlib"
    xmlns:local="clr-namespace:MVS"
    mc:Ignorable="d"
    d:DesignHeight="1280"
    d:DesignWidth="1920">

    <UserControl.Resources>

        <sys:Double
            x:Key="ChartWidth">1600</sys:Double>
        <sys:Double
            x:Key="ChartHeight">200</sys:Double>

        <sys:Double
            x:Key="lineLegendWidth">2</sys:Double>
        <sys:Double
            x:Key="lineLegendWidthTitle">4</sys:Double>
        <sys:Double
            x:Key="lineLegendHeight">2</sys:Double>

        <local:LabelAbsConverter
            x:Key="LabelAbsConverter" />

    </UserControl.Resources>

    <Grid>

        <Grid.RowDefinitions>
            <RowDefinition
                Height="*" />
            <RowDefinition
                Height="*" />
            <RowDefinition
                Height="*" />
        </Grid.RowDefinitions>

        <!--===========================================================-->
        <!--===========================================================-->
        <!--===========================================================-->
        <!--PITCH-->
        <!--===========================================================-->
        <!--===========================================================-->
        <!--===========================================================-->
        <telerik:GroupBox
            Grid.Row="0"
            Header="Pitch"
            Margin="8">

            <Grid>

                <Grid.ColumnDefinitions>
                    <ColumnDefinition
                        Width="*" />
                    <ColumnDefinition
                        Width="*" />
                    <ColumnDefinition
                        Width="*" />
                    <ColumnDefinition
                        Width="*" />
                </Grid.ColumnDefinitions>

                <Grid
                    Grid.Column="0"
                    Margin="8">

                    <Grid.ColumnDefinitions>
                        <ColumnDefinition
                            Width="2*" />
                        <ColumnDefinition
                            Width="60*" />
                    </Grid.ColumnDefinitions>

                    <Grid.RowDefinitions>
                        <RowDefinition
                            Height="9*" />
                        <RowDefinition
                            Height="30" />
                    </Grid.RowDefinitions>

                    <!--PITCH-->
                    <Grid
                        Grid.Column="0"
                        Grid.Row="0">

                        <Grid.RowDefinitions>
                            <RowDefinition
                                Height="6*" />
                            <RowDefinition
                                Height="*" />
                            <RowDefinition
                                Height="6*" />
                        </Grid.RowDefinitions>

                        <Grid
                            Grid.Column="1"
                            Grid.RowSpan="3">

                            <Grid.RowDefinitions>
                                <RowDefinition
                                    Height="2*" />
                                <RowDefinition
                                    Height="1*" />
                                <RowDefinition
                                    Height="5*" />
                                <RowDefinition
                                    Height="1*" />
                                <RowDefinition
                                    Height="1*" />
                            </Grid.RowDefinitions>

                            <Viewbox
                                Grid.Row="1"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                VerticalAlignment="Center">

                                <TextBlock
                                    Text="U" />

                            </Viewbox>

                            <Viewbox
                                Grid.Row="3"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                VerticalAlignment="Center">

                                <TextBlock
                                    Text="D" />

                            </Viewbox>

                        </Grid>

                    </Grid>

                    <!--PITCH INPUT-->
                    <Grid
                        Grid.Column="1"
                        Grid.Row="0">

                        <Grid.RowDefinitions>
                            <RowDefinition
                                Height="Auto" />
                            <RowDefinition
                                Height="*" />
                        </Grid.RowDefinitions>

                        <TextBlock
                            Text="Pitch Input"
                            HorizontalAlignment="Center" />

                        <!--Pitch Input Chart-->
                        <telerik:RadCartesianChart
                            Grid.Row="1"
                            Name="chartPitch1"
                            Margin="10,0,0,0">

                            <!--Pitch Input Test-->
                            <telerik:LineSeries
                                ZIndex="200"
                                Stroke="{StaticResource ColorGraphLineTest}"
                                CategoryBinding="timestamp"
                                ValueBinding="graphData">

                                <telerik:LineSeries.RenderOptions>
                                    <telerik:XamlRenderOptions
                                        DefaultVisualsRenderMode="Batch" />
                                </telerik:LineSeries.RenderOptions>

                            </telerik:LineSeries>

                            <!--Pitch Input Ref-->
                            <telerik:LineSeries
                                ZIndex="200"
                                Stroke="{StaticResource ColorGraphLineRef}"
                                CategoryBinding="timestamp"
                                ValueBinding="graphData">

                                <telerik:LineSeries.RenderOptions>
                                    <telerik:XamlRenderOptions
                                        DefaultVisualsRenderMode="Batch" />
                                </telerik:LineSeries.RenderOptions>

                            </telerik:LineSeries>

                            <telerik:RadCartesianChart.HorizontalAxis>
                                <telerik:DateTimeContinuousAxis
                                    MajorStepUnit="Minute"
                                    MajorStep="5"
                                    ShowLabels="False"
                                    PlotMode="OnTicks"
                                    TickOrigin="{Binding alignmentTime}"
                                    Minimum="{Binding alignmentTimeMin}"
                                    Maximum="{Binding alignmentTimeMax}" />
                            </telerik:RadCartesianChart.HorizontalAxis>

                            <telerik:RadCartesianChart.VerticalAxis>
                                <telerik:LinearAxis
                                    x:Name="chartPitchVerticalAxis"
                                    Maximum="{Binding pitchChartAxisMax}"
                                    Minimum="{Binding pitchChartAxisMin}">

                                    <telerik:LinearAxis.LabelTemplate>
                                        <DataTemplate>
                                            <TextBlock
                                                Text="{Binding Converter={StaticResource LabelAbsConverter}}" />
                                        </DataTemplate>
                                    </telerik:LinearAxis.LabelTemplate>

                                </telerik:LinearAxis>
                            </telerik:RadCartesianChart.VerticalAxis>

                            <telerik:RadCartesianChart.Grid>
                                <telerik:CartesianChartGrid
                                    MajorLinesVisibility="XY" />
                            </telerik:RadCartesianChart.Grid>

                            <telerik:RadCartesianChart.Annotations>

                                <telerik:CartesianGridLineAnnotation
                                    Axis="{Binding ElementName=chartPitchVerticalAxis}"
                                    Value="0"
                                    Stroke="Black"
                                    StrokeThickness="2" />

                            </telerik:RadCartesianChart.Annotations>

                        </telerik:RadCartesianChart>

                    </Grid>

                    <!--Pitch Input X-axis labels-->
                    <Grid
                        Grid.Column="1"
                        Grid.Row="1"
                        Margin="20,0,0,0">

                        <Grid.ColumnDefinitions>
                            <ColumnDefinition
                                Width="*" />
                            <ColumnDefinition
                                Width="*" />
                            <ColumnDefinition
                                Width="*" />
                            <ColumnDefinition
                                Width="*" />
                            <ColumnDefinition
                                Width="*" />
                            <ColumnDefinition
                                Width="*" />
                            <ColumnDefinition
                                Width="*" />
                            <ColumnDefinition
                                Width="*" />
                        </Grid.ColumnDefinitions>

                        <Viewbox
                            Grid.Column="0"
                            Grid.ColumnSpan="2"
                            StretchDirection="Both"
                            Stretch="Uniform"
                            HorizontalAlignment="Left">

                            <Label
                                Content="-20 mins" />

                        </Viewbox>

                        <Viewbox
                            Grid.Column="1"
                            Grid.ColumnSpan="2"
                            StretchDirection="Both"
                            Stretch="Uniform"
                            HorizontalAlignment="Center">

                            <Label
                                Content="-15" />

                        </Viewbox>

                        <Viewbox
                            Grid.Column="3"
                            Grid.ColumnSpan="2"
                            StretchDirection="Both"
                            Stretch="Uniform"
                            HorizontalAlignment="Center">

                            <Label
                                Content="-10" />

                        </Viewbox>

                        <Viewbox
                            Grid.Column="5"
                            Grid.ColumnSpan="2"
                            StretchDirection="Both"
                            Stretch="Uniform"
                            HorizontalAlignment="Center">

                            <Label
                                Content="-5" />

                        </Viewbox>

                        <Viewbox
                            Grid.Column="10"
                            StretchDirection="Both"
                            Stretch="Uniform"
                            HorizontalAlignment="Right">

                            <Label
                                Content="0" />

                        </Viewbox>

                    </Grid>

                </Grid>

                <Grid
                    Grid.Column="1"
                    Margin="8">

                    <Grid.ColumnDefinitions>
                        <ColumnDefinition
                            Width="2*" />
                        <ColumnDefinition
                            Width="60*" />
                    </Grid.ColumnDefinitions>

                    <Grid.RowDefinitions>
                        <RowDefinition
                            Height="9*" />
                        <RowDefinition
                            Height="30" />
                    </Grid.RowDefinitions>

                    <!--PITCH MEAN-->
                    <Grid
                        Grid.Column="0"
                        Grid.Row="0">

                        <Grid.RowDefinitions>
                            <RowDefinition
                                Height="5*" />
                            <RowDefinition
                                Height="*" />
                            <RowDefinition
                                Height="*" />
                            <RowDefinition
                                Height="6*" />
                        </Grid.RowDefinitions>

                        <Grid
                            Grid.Column="1"
                            Grid.RowSpan="4">

                            <Grid.RowDefinitions>
                                <RowDefinition
                                    Height="2*" />
                                <RowDefinition
                                    Height="1*" />
                                <RowDefinition
                                    Height="5*" />
                                <RowDefinition
                                    Height="1*" />
                                <RowDefinition
                                    Height="1*" />
                            </Grid.RowDefinitions>

                            <Viewbox
                                Grid.Row="1"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                VerticalAlignment="Center">

                                <TextBlock
                                    Text="U" />

                            </Viewbox>

                            <Viewbox
                                Grid.Row="3"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                VerticalAlignment="Center">

                                <TextBlock
                                    Text="D" />

                            </Viewbox>

                        </Grid>

                    </Grid>

                    <Grid
                        Grid.Column="1"
                        Grid.Row="0">

                        <Grid.RowDefinitions>
                            <RowDefinition
                                Height="Auto" />
                            <RowDefinition
                                Height="*" />
                        </Grid.RowDefinitions>

                        <!--Chart header-->
                        <TextBlock
                            Text="Mean Pitch"
                            HorizontalAlignment="Center" />

                        <!--Pitch Mean Chart-->
                        <telerik:RadCartesianChart
                            Grid.Row="1"
                            Name="chartPitch2"
                            Margin="10,0,0,0">

                            <!--Pitch Mean Test-->
                            <telerik:LineSeries
                                ZIndex="200"
                                Stroke="{StaticResource ColorGraphLineTest}"
                                CategoryBinding="timestamp"
                                ValueBinding="graphData">

                                <telerik:LineSeries.RenderOptions>
                                    <telerik:XamlRenderOptions
                                        DefaultVisualsRenderMode="Batch" />
                                </telerik:LineSeries.RenderOptions>

                            </telerik:LineSeries>

                            <!--Pitch Mean Ref-->
                            <telerik:LineSeries
                                ZIndex="200"
                                Stroke="{StaticResource ColorGraphLineRef}"
                                CategoryBinding="timestamp"
                                ValueBinding="graphData">

                                <telerik:LineSeries.RenderOptions>
                                    <telerik:XamlRenderOptions
                                        DefaultVisualsRenderMode="Batch" />
                                </telerik:LineSeries.RenderOptions>

                            </telerik:LineSeries>

                            <telerik:RadCartesianChart.HorizontalAxis>
                                <telerik:DateTimeContinuousAxis
                                    MajorStepUnit="Minute"
                                    MajorStep="5"
                                    ShowLabels="False"
                                    PlotMode="OnTicks"
                                    TickOrigin="{Binding alignmentTime}"
                                    Minimum="{Binding alignmentTimeMin}"
                                    Maximum="{Binding alignmentTimeMax}" />
                            </telerik:RadCartesianChart.HorizontalAxis>

                            <telerik:RadCartesianChart.VerticalAxis>
                                <telerik:LinearAxis
                                    x:Name="chartPitchVerticalAxis1"
                                    Maximum="{Binding meanPitchChartAxisMax}"
                                    Minimum="{Binding meanPitchChartAxisMin}">

                                    <telerik:LinearAxis.LabelTemplate>
                                        <DataTemplate>
                                            <TextBlock
                                                Text="{Binding Converter={StaticResource LabelAbsConverter}}" />
                                        </DataTemplate>
                                    </telerik:LinearAxis.LabelTemplate>

                                </telerik:LinearAxis>
                            </telerik:RadCartesianChart.VerticalAxis>

                            <telerik:RadCartesianChart.Grid>
                                <telerik:CartesianChartGrid
                                    MajorLinesVisibility="XY" />
                            </telerik:RadCartesianChart.Grid>

                            <telerik:RadCartesianChart.Annotations>

                                <telerik:CartesianGridLineAnnotation
                                    Axis="{Binding ElementName=chartPitchVerticalAxis1}"
                                    Value="0"
                                    Stroke="Black"
                                    StrokeThickness="2" />

                            </telerik:RadCartesianChart.Annotations>

                        </telerik:RadCartesianChart>

                    </Grid>

                    <!--Pitch Mean X-axis labels-->
                    <Grid
                        Grid.Column="1"
                        Grid.Row="1"
                        Margin="20,0,0,0">

                        <Grid.ColumnDefinitions>
                            <ColumnDefinition
                                Width="*" />
                            <ColumnDefinition
                                Width="*" />
                            <ColumnDefinition
                                Width="*" />
                            <ColumnDefinition
                                Width="*" />
                            <ColumnDefinition
                                Width="*" />
                            <ColumnDefinition
                                Width="*" />
                            <ColumnDefinition
                                Width="*" />
                            <ColumnDefinition
                                Width="*" />
                        </Grid.ColumnDefinitions>

                        <Viewbox
                            Grid.Column="0"
                            Grid.ColumnSpan="2"
                            StretchDirection="Both"
                            Stretch="Uniform"
                            HorizontalAlignment="Left">

                            <Label
                                Content="-20 mins" />

                        </Viewbox>

                        <Viewbox
                            Grid.Column="1"
                            Grid.ColumnSpan="2"
                            StretchDirection="Both"
                            Stretch="Uniform"
                            HorizontalAlignment="Center">

                            <Label
                                Content="-15" />

                        </Viewbox>

                        <Viewbox
                            Grid.Column="3"
                            Grid.ColumnSpan="2"
                            StretchDirection="Both"
                            Stretch="Uniform"
                            HorizontalAlignment="Center">

                            <Label
                                Content="-10" />

                        </Viewbox>

                        <Viewbox
                            Grid.Column="5"
                            Grid.ColumnSpan="2"
                            StretchDirection="Both"
                            Stretch="Uniform"
                            HorizontalAlignment="Center">

                            <Label
                                Content="-5" />

                        </Viewbox>

                        <Viewbox
                            Grid.Column="10"
                            StretchDirection="Both"
                            Stretch="Uniform"
                            HorizontalAlignment="Right">

                            <Label
                                Content="0" />

                        </Viewbox>

                    </Grid>

                </Grid>

                <Grid
                    Grid.Column="2"
                    Margin="8">

                    <Grid.ColumnDefinitions>
                        <ColumnDefinition
                            Width="2*" />
                        <ColumnDefinition
                            Width="60*" />
                    </Grid.ColumnDefinitions>

                    <Grid.RowDefinitions>
                        <RowDefinition
                            Height="9*" />
                        <RowDefinition
                            Height="30" />
                    </Grid.RowDefinitions>

                    <!--MEAN PITCH DEVIATION-->
                    <Grid
                        Grid.Column="0"
                        Grid.Row="0">

                        <Grid.RowDefinitions>
                            <RowDefinition
                                Height="5*" />
                            <RowDefinition
                                Height="*" />
                            <RowDefinition
                                Height="*" />
                            <RowDefinition
                                Height="6*" />
                        </Grid.RowDefinitions>

                        <Grid
                            Grid.Column="1"
                            Grid.RowSpan="4">

                            <Grid.RowDefinitions>
                                <RowDefinition
                                    Height="2*" />
                                <RowDefinition
                                    Height="1*" />
                                <RowDefinition
                                    Height="5*" />
                                <RowDefinition
                                    Height="1*" />
                                <RowDefinition
                                    Height="1*" />
                            </Grid.RowDefinitions>

                            <Viewbox
                                Grid.Row="1"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                VerticalAlignment="Center">

                                <TextBlock
                                    Text="U" />

                            </Viewbox>

                            <Viewbox
                                Grid.Row="3"
                                StretchDirection="Both"
                                Stretch="Uniform"
                                VerticalAlignment="Center">

                                <TextBlock
                                    Text="D" />

                            </Viewbox>

                        </Grid>

                    </Grid>

                    <Grid
                        Grid.Column="1"
                        Grid.Row="0">

                        <Grid.RowDefinitions>
                            <RowDefinition
                                Height="Auto" />
                            <RowDefinition
                                Height="*" />
                        </Grid.RowDefinitions>

                        <!--Chart header-->
                        <TextBlock
                            Text="Mean Pitch Deviation"
                            HorizontalAlignment="Center" />

                        <!--Mean Pitch Deviation Chart-->
                        <telerik:RadCartesianChart
                            Grid.Row="1"
                            Name="chartPitch3"
                            Margin="10,0,0,0">

                            <!--Deviation Pitch-->
                            <telerik:LineSeries
                                ZIndex="200"
                                Stroke="{StaticResource ColorGraphLineDev}"
                                CategoryBinding="timestamp"
                                ValueBinding="graphData">

                                <telerik:LineSeries.RenderOptions>
                                    <telerik:XamlRenderOptions
                                        DefaultVisualsRenderMode="Batch" />
                                </telerik:LineSeries.RenderOptions>

                            </telerik:LineSeries>

                            <telerik:RadCartesianChart.HorizontalAxis>
                                <telerik:DateTimeContinuousAxis
                                    MajorStepUnit="Minute"
                                    MajorStep="5"
                                    ShowLabels="False"
                                    PlotMode="OnTicks"
                                    TickOrigin="{Binding alignmentTime}"
                                    Minimum="{Binding alignmentTimeMin}"
                                    Maximum="{Binding alignmentTimeMax}" />
                            </telerik:RadCartesianChart.HorizontalAxis>

                            <telerik:RadCartesianChart.VerticalAxis>
                                <telerik:LinearAxis
                                    x:Name="chartPitchVerticalAxis3"
                                    Maximum="{Binding devPitchChartAxisMax}"
                                    Minimum="{Binding devPitchChartAxisMin}">

                                    <telerik:LinearAxis.LabelTemplate>
                                        <DataTemplate>
                                            <TextBlock
                                                Text="{Binding Converter={StaticResource LabelAbsConverter}}" />
                                        </DataTemplate>
                                    </telerik:LinearAxis.LabelTemplate>

                                </telerik:LinearAxis>
                            </telerik:RadCartesianChart.VerticalAxis>

                            <telerik:RadCartesianChart.Grid>
                                <telerik:CartesianChartGrid
                                    MajorLinesVisibility="XY" />
                            </telerik:RadCartesianChart.Grid>

                            <telerik:RadCartesianChart.Annotations>

                                <telerik:CartesianGridLineAnnotation
                                    Axis="{Binding ElementName=chartPitchVerticalAxis3}"
                                    Value="0"
                                    Stroke="Black"
                                    StrokeThickness="2" />

                            </telerik:RadCartesianChart.Annotations>

                        </telerik:RadCartesianChart>

                    </Grid>

                    <!--Pitch Deviation X-axis labels-->
                    <Grid
                        Grid.Column="1"
                        Grid.Row="1"
                        Margin="20,0,0,0">

                        <Grid.ColumnDefinitions>
                            <ColumnDefinition
                                Width="*" />
                            <ColumnDefinition
                                Width="*" />
                            <ColumnDefinition
                                Width="*" />
                            <ColumnDefinition
                                Width="*" />
                            <ColumnDefinition
                                Width="*" />
                            <ColumnDefinition
                                Width="*" />
                            <ColumnDefinition
                                Width="*" />
                            <ColumnDefinition
                                Width="*" />
                        </Grid.ColumnDefinitions>

                        <Viewbox
                            Grid.Column="0"
                            Grid.ColumnSpan="2"
                            StretchDirection="Both"
                            Stretch="Uniform"
                            HorizontalAlignment="Left">

                            <Label
                                Content="-20 mins" />

                        </Viewbox>

                        <Viewbox
                            Grid.Column="1"
                            Grid.ColumnSpan="2"
                            StretchDirection="Both"
                            Stretch="Uniform"
                            HorizontalAlignment="Center">

                            <Label
                                Content="-15" />

                        </Viewbox>

                        <Viewbox
                            Grid.Column="3"
                            Grid.ColumnSpan="2"
                            StretchDirection="Both"
                            Stretch="Uniform"
                            HorizontalAlignment="Center">

                            <Label
                                Content="-10" />

                        </Viewbox>

                        <Viewbox
                            Grid.Column="5"
                            Grid.ColumnSpan="2"
                            StretchDirection="Both"
                            Stretch="Uniform"
                            HorizontalAlignment="Center">

                            <Label
                                Content="-5" />

                        </Viewbox>

                        <Viewbox
                            Grid.Column="10"
                            StretchDirection="Both"
                            Stretch="Uniform"
                            HorizontalAlignment="Right">

                            <Label
                                Content="0" />

                        </Viewbox>

                    </Grid>

                </Grid>

                <!--Pitch Data Readouts-->
                <Grid
                    Grid.Column="3"
                    Margin="8">

                    <Grid.RowDefinitions>
                        <RowDefinition
                            Height="2*" />
                        <RowDefinition
                            Height="*" />
                        <RowDefinition
                            Height="*" />
                        <RowDefinition
                            Height="*" />
                        <RowDefinition
                            Height="*" />
                        <RowDefinition
                            Height="*" />
                        <RowDefinition
                            Height="*" />
                        <RowDefinition
                            Height="*" />
                        <RowDefinition
                            Height="*" />
                        <RowDefinition
                            Height="*" />
                        <RowDefinition
                            Height="*" />
                        <RowDefinition
                            Height="*" />
                    </Grid.RowDefinitions>

                    <Grid.ColumnDefinitions>
                        <ColumnDefinition
                            Width="3*" />
                        <ColumnDefinition
                            Width="2*" />
                        <ColumnDefinition
                            Width="2*" />
                        <ColumnDefinition
                            Width="2*" />
                    </Grid.ColumnDefinitions>

                    <!--Pitch Line Legend-->
                    <Viewbox
                        Grid.Row="2"
                        Grid.Column="1"
                        StretchDirection="Both"
                        Stretch="Uniform"
                        HorizontalAlignment="Center">

                        <Rectangle
                            Margin="1"
                            Width="{StaticResource lineLegendWidth}"
                            Height="{StaticResource lineLegendHeight}"
                            Fill="{StaticResource ColorGraphLineRef}" />

                    </Viewbox>

                    <Viewbox
                        Grid.Row="2"
                        Grid.Column="2"
                        StretchDirection="Both"
                        Stretch="Uniform"
                        HorizontalAlignment="Center">

                        <Rectangle
                            Margin="1"
                            Width="{StaticResource lineLegendWidth}"
                            Height="{StaticResource lineLegendHeight}"
                            Fill="{StaticResource ColorGraphLineTest}" />

                    </Viewbox>

                    <Viewbox
                        Grid.Row="2"
                        Grid.Column="3"
                        StretchDirection="Both"
                        Stretch="Uniform"
                        HorizontalAlignment="Center">

                        <Rectangle
                            Margin="1"
                            Width="{StaticResource lineLegendWidth}"
                            Height="{StaticResource lineLegendHeight}"
                            Fill="{StaticResource ColorGraphLineDev}" />

                    </Viewbox>

                    <!--Pitch Title-->
                    <Grid
                        Grid.Row="2"
                        Grid.RowSpan="2"
                        Grid.Column="0">

                        <Viewbox
                            Grid.Column="1"
                            StretchDirection="Both"
                            Stretch="Uniform"
                            HorizontalAlignment="Left">

                            <TextBlock
                                Text="Pitch" />

                        </Viewbox>

                    </Grid>

                    <!--Pitch Labels-->
                    <Separator
                        Grid.Row="3"
                        Grid.Column="0"
                        Grid.ColumnSpan="4"
                        VerticalAlignment="Bottom" />

                    <Viewbox
                        Grid.Row="3"
                        Grid.Column="1"
                        StretchDirection="Both"
                        Stretch="Uniform"
                        HorizontalAlignment="Center">

                        <TextBlock
                            Text="Ref" />

                    </Viewbox>

                    <Viewbox
                        Grid.Row="3"
                        Grid.Column="2"
                        StretchDirection="Both"
                        Stretch="Uniform"
                        HorizontalAlignment="Center">

                        <TextBlock
                            Text="Test" />

                    </Viewbox>

                    <Viewbox
                        Grid.Row="3"
                        Grid.Column="3"
                        StretchDirection="Both"
                        Stretch="Uniform"
                        HorizontalAlignment="Center">

                        <TextBlock
                            Text="Deviation" />

                    </Viewbox>

                    <!--Max Pitch up-->
                    <Viewbox
                        Grid.Row="4"
                        Grid.Column="0"
                        StretchDirection="Both"
                        Stretch="Uniform"
                        HorizontalAlignment="Left">

                        <TextBlock
                            Text="Max" />

                    </Viewbox>

                    <Viewbox
                        Grid.Row="4"
                        Grid.Column="1"
                        StretchDirection="Both"
                        Stretch="Uniform"
                        HorizontalAlignment="Center">

                        <TextBlock
                            Text="{Binding Path=refPitchMaxUpString}"
                            FontSize="{StaticResource fontSizeData}" />

                    </Viewbox>

                    <Viewbox
                        Grid.Row="4"
                        Grid.Column="2"
                        StretchDirection="Both"
                        Stretch="Uniform"
                        HorizontalAlignment="Center">

                        <TextBlock
                            Text="{Binding Path=testPitchMaxUpString}"
                            FontSize="{StaticResource fontSizeData}" />

                    </Viewbox>

                    <!--Max Pitch Down-->
                    <Viewbox
                        Grid.Row="5"
                        Grid.Column="1"
                        StretchDirection="Both"
                        Stretch="Uniform"
                        HorizontalAlignment="Center">

                        <TextBlock
                            Text="{Binding Path=refPitchMaxDownString}"
                            FontSize="{StaticResource fontSizeData}" />

                    </Viewbox>

                    <Viewbox
                        Grid.Row="5"
                        Grid.Column="2"
                        StretchDirection="Both"
                        Stretch="Uniform"
                        HorizontalAlignment="Center">

                        <TextBlock
                            Text="{Binding Path=testPitchMaxDownString}"
                            FontSize="{StaticResource fontSizeData}" />

                    </Viewbox>

                    <!--Max Pitch Deviation-->
                    <Viewbox
                        Grid.Row="4"
                        Grid.Column="3"
                        StretchDirection="Both"
                        Stretch="Uniform"
                        HorizontalAlignment="Center">

                        <TextBlock
                            Text="{Binding Path=devPitchMaxString}"
                            FontSize="{StaticResource fontSizeData}" />

                    </Viewbox>

                    <!--Mean Pitch-->
                    <Viewbox
                        Grid.Row="7"
                        Grid.Column="0"
                        StretchDirection="Both"
                        Stretch="Uniform"
                        HorizontalAlignment="Left">

                        <TextBlock
                            Text="Mean" />

                    </Viewbox>

                    <Viewbox
                        Grid.Row="7"
                        Grid.Column="1"
                        StretchDirection="Both"
                        Stretch="Uniform"
                        HorizontalAlignment="Center">

                        <TextBlock
                            Text="{Binding Path=refPitchMeanDataString}"
                            FontSize="{StaticResource fontSizeData}" />

                    </Viewbox>

                    <Viewbox
                        Grid.Row="7"
                        Grid.Column="2"
                        StretchDirection="Both"
                        Stretch="Uniform"
                        HorizontalAlignment="Center">

                        <TextBlock
