                        <Viewbox
                            Grid.Column="5"
                            Grid.ColumnSpan="2"
                            StretchDirection="Both"
                            Stretch="Uniform"
                            HorizontalAlignment="Center">

                            <Label
                                Content="-5" />

                        </Viewbox>

                        <Viewbox
                            Grid.Column="10"
                            StretchDirection="Both"
                            Stretch="Uniform"
                            HorizontalAlignment="Right">

                            <Label
                                Content="0" />

                        </Viewbox>

