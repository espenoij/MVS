                                MajorStepUnit="Minute"
                                MajorStep="5"
                                ShowLabels="False"
                                PlotMode="OnTicks"
                                TickOrigin="{Binding alignmentTime}"
                                Minimum="{Binding alignmentTimeMin20m}"
                                Maximum="{Binding alignmentTimeMax20m}" />
                        </telerik:RadCartesianChart.HorizontalAxis>

                        <telerik:RadCartesianChart.VerticalAxis>
                            <telerik:LinearAxis
                                x:Name="chartHeaveVerticalAxis20m"
                                Maximum="{Binding heaveChartAxisMax20m}"
                                Minimum="{Binding heaveChartAxisMin20m}" />
                        </telerik:RadCartesianChart.VerticalAxis>

                        <telerik:RadCartesianChart.Grid>
                            <telerik:CartesianChartGrid
                                MajorLinesVisibility="XY" />
                        </telerik:RadCartesianChart.Grid>

                        <telerik:RadCartesianChart.Annotations>

                            <telerik:CartesianGridLineAnnotation
                                Axis="{Binding ElementName=chartHeaveVerticalAxis20m}"
                                Value="0"
                                Stroke="Black"
                                StrokeThickness="2" />

                        </telerik:RadCartesianChart.Annotations>

                    </telerik:RadCartesianChart>


                    <!--Heave X-axis labels-->
                    <Grid
                        Grid.Column="1"
                        Grid.Row="7"
                        Margin="20,0,0,-1">

                        <Grid.ColumnDefinitions>
                            <ColumnDefinition
                                Width="*" />
                            <ColumnDefinition
                                Width="*" />
                            <ColumnDefinition
                                Width="*" />
                            <ColumnDefinition
                                Width="*" />
                            <ColumnDefinition
                                Width="*" />
                            <ColumnDefinition
                                Width="*" />
                            <ColumnDefinition
                                Width="*" />
                            <ColumnDefinition
                                Width="*" />
                        </Grid.ColumnDefinitions>

                        <Viewbox
                            Grid.Column="0"
                            Grid.ColumnSpan="2"
                            StretchDirection="Both"
                            Stretch="Uniform"
                            HorizontalAlignment="Left">

                            <Label
