<UserControl
    x:Class="SensorMonitorClient.HelideckReport_CAP"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:telerik="http://schemas.telerik.com/2008/xaml/presentation"
    mc:Ignorable="d"
    d:DesignHeight="1080"
    d:DesignWidth="1920">

    <!--Advarsel! Rapporten i denne filen er designet for utskift til PDF.
    Enkelte tagger er ikke tilgjengelig i koden som skriver ut til PDF.
    Alle endringer i rapporten må sjekkes i utskrevet PDF.-->

    <Grid>

        <Grid.ColumnDefinitions>
            <ColumnDefinition
                Width="4*" />
            <ColumnDefinition
                Width="4*" />
            <ColumnDefinition
                Width="1*" />
        </Grid.ColumnDefinitions>

        <Grid
            Grid.Column="0">

            <Grid.ColumnDefinitions>
                <ColumnDefinition
                    Width="*" />
                <ColumnDefinition
                    Width="*" />
            </Grid.ColumnDefinitions>

            <Grid.RowDefinitions>
                <RowDefinition
                    Height="*" />
                <RowDefinition
                    Height="*" />
                <RowDefinition
                    Height="*" />
            </Grid.RowDefinitions>

            <!--OFFSHORE WEATHER REPORT-->
            <telerik:GroupBox
                Header="OFFSHORE WEATHER REPORT"
                Grid.Row="0"
                Margin="8">

                <DockPanel>

                    <telerik:RadButton
                        DockPanel.Dock="Bottom"
                        Name="btnClearHelideckReport"
                        Content="Clear"
                        HorizontalAlignment="Right"
                        Width="110"
                        Click="btnClearHelideckReport_Click" />

                    <StackPanel
                        Margin="8">

                        <!--Report prepared by-->
                        <DockPanel>

                            <telerik:Label
                                Width="150"
                                Content="Report prepared by"
                                VerticalAlignment="Center" />

                            <TextBox
                                Name="tbNameOfHLO"
                                Text="{Binding Path=nameOfHLO}"
                                LostFocus="tbNameOfHLO_LostFocus"
                                KeyDown="tbNameOfHLO_KeyDown" />

                        </DockPanel>

                    </StackPanel>

                </DockPanel>

            </telerik:GroupBox>

            <!--OBSERVATIONS-->
            <telerik:GroupBox
                Header="OBSERVATIONS"
                Grid.Column="1"
                Grid.Row="0"
                Margin="8">

                <DockPanel>

                    <telerik:RadButton
                        DockPanel.Dock="Bottom"
                        Name="btnClearOtherWeatherInfo"
                        Content="Clear"
                        HorizontalAlignment="Right"
                        Width="110"
                        Click="btnClearObservations_Click" />

                    <StackPanel
                        DockPanel.Dock="Top"
                        Margin="8">

                        <!--Lightning Present-->
                        <DockPanel>

                            <telerik:Label
                                Content="Lightning Present"
                                VerticalAlignment="Center" />

                            <CheckBox
                                IsChecked="{Binding Path=lightningPresent, Mode=TwoWay}"
                                VerticalAlignment="Center"
                                HorizontalAlignment="Right" />

                        </DockPanel>

                        <!--Cold Flaring-->
                        <DockPanel>

                            <telerik:Label
                                Content="Cold Flaring"
                                VerticalAlignment="Center" />

                            <CheckBox
                                IsChecked="{Binding Path=coldFlaring, Mode=TwoWay}"
                                VerticalAlignment="Center"
                                HorizontalAlignment="Right" />

                        </DockPanel>

                    </StackPanel>

                </DockPanel>

            </telerik:GroupBox>

            <!--EMAIL-->
            <telerik:GroupBox
                Header="EMAIL"
                Grid.Column="1"
                Grid.Row="2"
                Margin="8">

                <DockPanel>

                    <telerik:RadButton
                        DockPanel.Dock="Bottom"
                        Name="btnClearEmail"
                        Content="Clear"
                        HorizontalAlignment="Right"
                        Width="110"
                        Click="btnClearEmail_Click" />

                    <StackPanel
                        DockPanel.Dock="Top"
                        Margin="8">

                        <!--Email-->
                        <DockPanel>

                            <telerik:Label
                                Width="150"
                                Content="Email: From"
                                VerticalAlignment="Center" />

                            <TextBox
                                Name="tbEmail"
                                Text="{Binding Path=emailFrom}"
                                LostFocus="tbEmail_LostFocus"
                                KeyDown="tbEmail_KeyDown" />

                        </DockPanel>

                        <!--Send To-->
                        <DockPanel>

                            <telerik:Label
                                Width="150"
                                Content="Email: To"
                                VerticalAlignment="Center" />

                            <TextBox
                                Name="tbSendTo"
                                Text="{Binding Path=emailTo}"
                                LostFocus="tbSendTo_LostFocus"
                                KeyDown="tbSendTo_KeyDown" />

                        </DockPanel>

                        <!--CC To-->
                        <DockPanel>

                            <telerik:Label
                                Width="150"
                                Content="Email: CC"
                                VerticalAlignment="Center" />

                            <TextBox
                                Name="tbCCTo"
                                Text="{Binding Path=emailCC}"
                                LostFocus="tbCCTo_LostFocus"
                                KeyDown="tbCCTo_KeyDown" />

                        </DockPanel>

                    </StackPanel>

                </DockPanel>

            </telerik:GroupBox>

            <!--SERVICES-->
            <telerik:GroupBox
                Header="SERVICES"
                Grid.Column="0"
                Grid.Row="1"
                Margin="8">

                <DockPanel>

                    <telerik:RadButton
                        DockPanel.Dock="Bottom"
                        Name="btnClearServices"
                        Content="Clear"
                        HorizontalAlignment="Right"
                        Width="110"
                        Click="btnClearServices_Click" />

                    <StackPanel
                        DockPanel.Dock="Top"
                        Margin="8">

                        <!--Rescue & Recovery Available-->
                        <DockPanel>

                            <telerik:Label
                                Content="Rescue &amp; Recovery Available"
                                VerticalAlignment="Center" />

                            <CheckBox
                                IsChecked="{Binding Path=rescueRecoveryAvailable, Mode=TwoWay}"
                                VerticalAlignment="Center"
                                HorizontalAlignment="Right" />

                        </DockPanel>

                        <!--Helifuel Serviceable-->
                        <DockPanel>

                            <telerik:Label
                                Content="Fuel Serviceable"
                                VerticalAlignment="Center" />

                            <CheckBox
                                IsChecked="{Binding Path=helifuelAvailable, Mode=TwoWay}"
                                VerticalAlignment="Center"
                                HorizontalAlignment="Right" />

                        </DockPanel>

                        <!--Fuel Quantity-->
                        <DockPanel>

                            <TextBox
                                DockPanel.Dock="Right"
                                Name="tbFuelQuantity"
                                Width="100"
                                Text="{Binding Path=fuelQuantity}"
                                LostFocus="tbFuelQuantity_LostFocus"
                                KeyDown="tbFuelQuantity_KeyDown" />

                            <telerik:Label
                                Content="Fuel Quantity"
                                VerticalAlignment="Center" />

                        </DockPanel>

                        <!--NDB Serviceable-->
                        <DockPanel>

                            <telerik:Label
                                Content="NDB Serviceable"
                                VerticalAlignment="Center" />

                            <CheckBox
                                IsChecked="{Binding Path=ndbServiceable, Mode=TwoWay}"
                                VerticalAlignment="Center"
                                HorizontalAlignment="Right" />

                        </DockPanel>

                    </StackPanel>

                </DockPanel>

            </telerik:GroupBox>

            <!--SENSORS-->
            <telerik:GroupBox
                Header="SENSORS"
                Grid.Column="1"
                Grid.Row="1"
                Margin="8">

                <DockPanel>

                    <telerik:RadButton
                        DockPanel.Dock="Bottom"
                        Name="btnClearSensorRemarks"
                        Content="Clear"
                        HorizontalAlignment="Right"
                        Width="110"
                        Click="btnClearSensorRemarks_Click" />

                    <StackPanel
                        DockPanel.Dock="Top"
                        Margin="8">

                        <!--Any unserviceable sensors-->
                        <DockPanel>

                            <telerik:Label
                                Content="Any unserviceable sensors"
                                VerticalAlignment="Center" />

                            <CheckBox
                                IsChecked="{Binding Path=anyUnserviceableSensors, Mode=TwoWay}"
                                VerticalAlignment="Center"
                                HorizontalAlignment="Right" />

                        </DockPanel>

                        <telerik:Label
                            Content="Details"
                            HorizontalAlignment="Left" />

                        <TextBlock
                            Margin="16,0,16,0"
                            Text="E.g. type of sensor and the date it becaming unserviceable"
                            FontSize="{StaticResource reportFontSizeComment}"
                            TextWrapping="Wrap"
                            VerticalAlignment="Center" />

                        <Grid
                            Height="150">

                            <TextBox
                                Name="tbAnyUnserviceableSensorsComments"
                                Margin="8"
                                Text="{Binding Path=anyUnserviceableSensorsComments}"
                                TextWrapping="Wrap"
                                VerticalAlignment="Bottom"
                                LostFocus="tbAnyUnserviceableSensorsComments_LostFocus"
                                KeyDown="tbAnyUnserviceableSensorsComments_KeyDown" />

                            <TextBlock
                                IsHitTestVisible="False"
                                Text="Enter sensor remarks here"
                                VerticalAlignment="Bottom"
                                HorizontalAlignment="Left"
                                Margin="18,0,0,18"
                                Foreground="DarkGray">

                                <TextBlock.Style>
                                    <Style
                                        TargetType="{x:Type TextBlock}">
                                        <Setter
                                            Property="Visibility"
                                            Value="Collapsed" />
                                        <Style.Triggers>
                                            <DataTrigger
                                                Binding="{Binding Text, ElementName=tbAnyUnserviceableSensorsComments}"
                                                Value="">
                                                <Setter
                                                    Property="Visibility"
                                                    Value="Visible" />
                                            </DataTrigger>
                                        </Style.Triggers>
                                    </Style>
                                </TextBlock.Style>
                            </TextBlock>
                        </Grid>

                    </StackPanel>

                </DockPanel>

            </telerik:GroupBox>

            <!--REMARKS-->
            <telerik:GroupBox
                Header="REMARKS"
                Grid.Column="0"
                Grid.Row="2"
                Margin="8">

                <DockPanel>

                    <telerik:RadButton
                        DockPanel.Dock="Bottom"
                        Name="btnClearRemarks"
                        Content="Clear"
                        HorizontalAlignment="Right"
                        Width="110"
                        Click="btnClearRemarks_Click" />

                    <StackPanel
                        DockPanel.Dock="Top"
                        Margin="8">

                        <telerik:Label
                            Content="Other relevant weather info"
                            HorizontalAlignment="Left" />

                        <TextBlock
                            Margin="16,0,16,0"
                            Text="E.g. fog banks, rapid changes, etc."
                            FontSize="{StaticResource reportFontSizeComment}"
                            TextWrapping="Wrap"
                            VerticalAlignment="Center" />

                        <Grid
                            Height="150">

                            <TextBox
                                Name="tbRemarks"
                                Margin="8"
                                Text="{Binding Path=otherWeatherInfo}"
                                TextWrapping="Wrap"
                                VerticalAlignment="Bottom"
                                LostFocus="tbRemarks_LostFocus"
                                KeyDown="tbRemarks_KeyDown" />

                            <TextBlock
                                IsHitTestVisible="False"
                                Text="Enter weather remarks here"
                                VerticalAlignment="Bottom"
                                HorizontalAlignment="Left"
                                Margin="18,0,0,18"
                                Foreground="DarkGray">

                                <TextBlock.Style>
                                    <Style
                                        TargetType="{x:Type TextBlock}">
                                        <Setter
                                            Property="Visibility"
                                            Value="Collapsed" />
                                        <Style.Triggers>
                                            <DataTrigger
                                                Binding="{Binding Text, ElementName=tbOtherWeatherInfo}"
                                                Value="">
                                                <Setter
                                                    Property="Visibility"
                                                    Value="Visible" />
                                            </DataTrigger>
                                        </Style.Triggers>
                                    </Style>
                                </TextBlock.Style>
                            </TextBlock>
                        </Grid>

                    </StackPanel>

                </DockPanel>

            </telerik:GroupBox>

        </Grid>

        <!--REPORT PREVIEW-->
        <telerik:GroupBox
            Header="PREVIEW"
            Grid.Column="1"
            Margin="8">

            <Grid>

                <Viewbox
                    Grid.Column="1"
                    Margin="8"
                    StretchDirection="Both"
                    Stretch="Uniform">

                    <Border
                        x:Name="helideckReportPreviewContainer"
                        Height="3508"
                        Width="2480">

                        <Grid
                            Margin="40">

                            <Grid.RowDefinitions>
                                <RowDefinition
                                    Height="1.1*" />
                                <RowDefinition
                                    Height="*" />
                                <RowDefinition
                                    Height="*" />
                                <RowDefinition
                                    Height="*" />
                                <RowDefinition
                                    Height="*" />
                                <RowDefinition
                                    Height="*" />
                                <RowDefinition
                                    Height="0.75*" />
                                <RowDefinition
                                    Height="*" />
                                <RowDefinition
                                    Height="*" />
                                <RowDefinition
                                    Height="*" />
                                <RowDefinition
                                    Height="*" />
                                <RowDefinition
                                    Height="*" />
                                <RowDefinition
                                    Height="*" />
                                <RowDefinition
                                    Height="*" />
                                <RowDefinition
                                    Height="*" />
                                <RowDefinition
                                    Height="*" />
                                <RowDefinition
                                    Height="*" />
                                <RowDefinition
                                    Height="*" />
                                <RowDefinition
                                    Height="*" />
                                <RowDefinition
                                    Height="*" />
                                <RowDefinition
                                    Height="*" />
                                <RowDefinition
                                    Height="*" />
                                <RowDefinition
                                    Height="*" />
                                <RowDefinition
                                    Height="*" />
                                <RowDefinition
                                    Height="*" />
                                <RowDefinition
                                    Height="*" />
                                <RowDefinition
                                    Height="*" />
                                <RowDefinition
                                    Height="*" />
                                <RowDefinition
                                    Height="*" />
                                <RowDefinition
                                    Height="*" />
                            </Grid.RowDefinitions>

                            <Grid.ColumnDefinitions>
                                <ColumnDefinition
                                    Width="3*" />
                                <ColumnDefinition
                                    Width="2*" />
                            </Grid.ColumnDefinitions>

                            <!--Title-->
                            <DockPanel
                                Grid.Row="0"
                                Grid.Column="0"
                                Grid.ColumnSpan="2"
                                Margin="{StaticResource reportLineMargin_CAP}"
                                Background="{StaticResource ColorReportBackground_CAP}">

                                <Border
                                    Margin="{StaticResource reportContentMargin_CAP}"
                                    Width="1400"
                                    Background="White"
                                    BorderBrush="{StaticResource ColorReportBorder_CAP}"
                                    BorderThickness="{StaticResource reportBorderThickness_CAP}">

                                    <TextBlock
                                        Text="OFFSHORE WEATHER REPORT"
                                        VerticalAlignment="Center"
                                        HorizontalAlignment="Center"
                                        FontSize="{StaticResource reportFontSizeHeader_CAP}" />

                                </Border>

                            </DockPanel>

                            <!--Name-->
                            <StackPanel
                                Grid.Row="1"
                                Grid.Column="0"
                                Orientation="Horizontal"
                                Margin="{StaticResource reportLineMargin_CAP}"
                                Background="{StaticResource ColorReportBackground_CAP}">

                                <TextBlock
                                    Text="Name"
                                    FontWeight="Bold"
                                    FontSize="{StaticResource reportFontSizeLabel_CAP}"
                                    VerticalAlignment="Center"
                                    Margin="{StaticResource reportContentMargin_CAP}" />

                                <Border
                                    Margin="{StaticResource reportContentMargin_CAP}"
                                    Width="1000"
                                    Background="White"
                                    BorderBrush="{StaticResource ColorReportBorder_CAP}"
                                    BorderThickness="{StaticResource reportBorderThickness_CAP}">

                                    <TextBlock
                                        Text="{Binding Path=vesselNameString}"
                                        VerticalAlignment="Center"
                                        HorizontalAlignment="Center"
                                        FontSize="{StaticResource reportFontSizeContent_CAP}" />

                                </Border>

                            </StackPanel>

                            <!--Vessel Heading-->
                            <StackPanel
                                Grid.Row="1"
                                Grid.Column="1"
                                Orientation="Horizontal"
                                Margin="{StaticResource reportLineMargin_CAP}"
                                Background="{StaticResource ColorReportBackground_CAP}">

                                <Border
                                    Margin="{StaticResource reportContentMargin_CAP}"
                                    Width="{StaticResource reportVerticalGuideRight_CAP}">

                                    <StackPanel
                                        VerticalAlignment="Center">
                                        
                                        <TextBlock
                                            Text="Vessel"
                                            FontWeight="Bold"
                                            FontSize="{StaticResource reportFontSizeLabel_CAP}"
                                            HorizontalAlignment="Right" />

                                        <TextBlock
                                            Text="Heading"
                                            FontWeight="Bold"
                                            FontSize="{StaticResource reportFontSizeLabel_CAP}"
                                            HorizontalAlignment="Right" />

                                    </StackPanel>

                                </Border>

                                <Border
                                    Margin="{StaticResource reportContentMargin_CAP}"
                                    Width="{StaticResource reportBoxWidth3}"
                                    Background="White"
                                    BorderBrush="{StaticResource ColorReportBorder_CAP}"
                                    BorderThickness="{StaticResource reportBorderThickness_CAP}">

                                    <TextBlock
                                        Text="{Binding Path=vesselHeadingString_CAP}"
                                        HorizontalAlignment="Center"
                                        VerticalAlignment="Center"
                                        FontSize="{StaticResource reportFontSizeContent_CAP}" />
                                
                                </Border>

                                <TextBlock
                                    DockPanel.Dock="Right"
                                    Text="degrees"
                                    FontSize="{StaticResource reportFontSizeLabel_CAP}"
                                    VerticalAlignment="Center"
                                    Margin="{StaticResource reportContentMargin_CAP}" />

                            </StackPanel>

                            <!--Date-->
                            <StackPanel
                                Grid.Row="2"
                                Grid.Column="0"
                                Orientation="Horizontal"
                                Margin="{StaticResource reportLineMargin_CAP}"
                                Background="{StaticResource ColorReportBackground_CAP}">

                                <Border
                                    Margin="{StaticResource reportContentMargin_CAP}"
                                    Width="{StaticResource reportVerticalGuideLeft_CAP}">

                                    <TextBlock
                                        Text="Date"
                                        FontWeight="Bold"
                                        VerticalAlignment="Center"
                                        HorizontalAlignment="Right"
                                        FontSize="{StaticResource reportFontSizeLabel_CAP}" />

                                </Border>

                                <Border
                                    Margin="{StaticResource reportContentMargin_CAP}"
                                    Background="White"
                                    Width="{StaticResource reportBoxWidth2}"
                                    BorderBrush="{StaticResource ColorReportBorder_CAP}"
                                    BorderThickness="{StaticResource reportBorderThickness_CAP}">

                                    <TextBlock
                                        Text="{Binding Path=dateDayString_CAP}"
                                        HorizontalAlignment="Center"
                                        VerticalAlignment="Center"
                                        FontSize="{StaticResource reportFontSizeContent_CAP}" />
                                    
                                </Border>

                                <TextBlock
                                    Text="/"
                                    VerticalAlignment="Center"
                                    FontSize="{StaticResource reportFontSizeLabel_CAP}"
                                    Margin="{StaticResource reportContentMargin_CAP}" />

                                <Border
                                    Margin="{StaticResource reportContentMargin_CAP}"
                                    Background="White"
                                    Width="{StaticResource reportBoxWidth2}"
                                    BorderBrush="{StaticResource ColorReportBorder_CAP}"
                                    BorderThickness="{StaticResource reportBorderThickness_CAP}">

                                    <TextBlock
                                        Text="{Binding Path=dateMonthString_CAP}"
                                        VerticalAlignment="Center"
                                        HorizontalAlignment="Center"
                                        FontSize="{StaticResource reportFontSizeContent_CAP}"/>

                                </Border>

                                <TextBlock
                                    Text="/"
                                    VerticalAlignment="Center"
                                    FontSize="{StaticResource reportFontSizeLabel_CAP}"
                                    Margin="{StaticResource reportContentMargin_CAP}" />

                                <Border
                                    Margin="{StaticResource reportContentMargin_CAP}"
                                    Background="White"
                                    Width="{StaticResource reportBoxWidth3}"
                                    BorderBrush="{StaticResource ColorReportBorder_CAP}"
                                    BorderThickness="{StaticResource reportBorderThickness_CAP}">

                                    <TextBlock
                                        Text="{Binding Path=dateYearString_CAP}"
                                        VerticalAlignment="Center"
                                        HorizontalAlignment="Center"
                                        FontSize="{StaticResource reportFontSizeContent_CAP}" />

                                </Border>

                            </StackPanel>

                            <!--Time-->
                            <StackPanel
                                Grid.Row="2"
                                Grid.Column="1"
                                Orientation="Horizontal"
                                Margin="{StaticResource reportLineMargin_CAP}"
                                Background="{StaticResource ColorReportBackground_CAP}">

                                <Border
                                    Margin="{StaticResource reportContentMargin_CAP}"
                                    Width="{StaticResource reportVerticalGuideRight_CAP}">

                                    <TextBlock
                                        Text="Time"
                                        FontWeight="Bold"
                                        VerticalAlignment="Center"
                                        HorizontalAlignment="Right"
                                        FontSize="{StaticResource reportFontSizeLabel_CAP}" />

                                </Border>

                                <Border
                                    Margin="{StaticResource reportContentMargin_CAP}"
                                    Background="White"
                                    Width="{StaticResource reportBoxWidth2}"
                                    BorderBrush="{StaticResource ColorReportBorder_CAP}"
                                    BorderThickness="{StaticResource reportBorderThickness_CAP}">

                                    <TextBlock
                                        Text="{Binding Path=clockHoursString_CAP}"
                                        HorizontalAlignment="Center"
                                        VerticalAlignment="Center"
                                        FontSize="{StaticResource reportFontSizeContent_CAP}" />

                                </Border>

                                <TextBlock
                                    Text=":"
                                    VerticalAlignment="Center"
                                    FontSize="{StaticResource reportFontSizeLabel_CAP}"
                                    Margin="{StaticResource reportContentMargin_CAP}" />

                                <Border
                                    Margin="{StaticResource reportContentMargin_CAP}"
                                    Background="White"
                                    Width="{StaticResource reportBoxWidth2}"
                                    BorderBrush="{StaticResource ColorReportBorder_CAP}"
                                    BorderThickness="{StaticResource reportBorderThickness_CAP}">

                                    <TextBlock
                                        Text="{Binding Path=clockMinutesString_CAP}"
                                        HorizontalAlignment="Center"
                                        VerticalAlignment="Center"
                                        FontSize="{StaticResource reportFontSizeContent_CAP}" />

                                </Border>

                                <TextBlock
                                    Text="UTC"
                                    VerticalAlignment="Center"
                                    FontSize="{StaticResource reportFontSizeLabel_CAP}"
                                    Margin="{StaticResource reportContentMargin_CAP}" />

                            </StackPanel>

                            <!--Latitude-->
                            <StackPanel
                                Grid.Row="3"
                                Grid.Column="0"
                                Orientation="Horizontal"
                                Margin="{StaticResource reportLineMargin_CAP}"
                                Background="{StaticResource ColorReportBackground_CAP}">

                                <Border
                                    Margin="{StaticResource reportContentMargin_CAP}"
                                    Width="{StaticResource reportVerticalGuideLeft_CAP}">

                                    <TextBlock
                                        Text="Latitude"
                                        FontWeight="Bold"
                                        VerticalAlignment="Center"
                                        HorizontalAlignment="Right"
                                        FontSize="{StaticResource reportFontSizeLabel_CAP}"
                                        Margin="{StaticResource reportContentMargin_CAP}" />

                                </Border>

                                <Border
                                    Margin="{StaticResource reportContentMargin_CAP}"
                                    Background="White"
                                    Width="{StaticResource reportBoxWidth1}"
                                    BorderBrush="{StaticResource ColorReportBorder_CAP}"
                                    BorderThickness="{StaticResource reportBorderThickness_CAP}">

                                    <TextBlock
                                        Text="{Binding Path=posLatHemisphereString_CAP}"
                                        HorizontalAlignment="Center"
                                        VerticalAlignment="Center"
                                        FontSize="{StaticResource reportFontSizeContent_CAP}"/>
                                    
                                </Border>

                                <Border
                                    Margin="{StaticResource reportContentMargin_CAP}"
                                    Background="White"
                                    Width="{StaticResource reportBoxWidth3}"
                                    BorderBrush="{StaticResource ColorReportBorder_CAP}"
                                    BorderThickness="{StaticResource reportBorderThickness_CAP}">

                                    <TextBlock
                                        Text="{Binding Path=posLatDegreesString_CAP}"
                                        HorizontalAlignment="Center"
                                        VerticalAlignment="Center"
                                        FontSize="{StaticResource reportFontSizeContent_CAP}" />

                                </Border>

                                <Border
                                    Margin="{StaticResource reportContentMargin_CAP}"
                                    Background="White"
                                    Width="{StaticResource reportBoxWidth4}"
                                    BorderBrush="{StaticResource ColorReportBorder_CAP}"
                                    BorderThickness="{StaticResource reportBorderThickness_CAP}">

                                    <TextBlock
                                        Text="{Binding Path=posLatMinutesString_CAP}"
                                        HorizontalAlignment="Center"
                                        VerticalAlignment="Center"
                                        FontSize="{StaticResource reportFontSizeContent_CAP}" />

                                </Border>

                            </StackPanel>

                            <!--Longitude-->
                            <StackPanel
                                Grid.Row="3"
                                Grid.Column="1"
                                Orientation="Horizontal"
                                Margin="{StaticResource reportLineMargin_CAP}"
                                Background="{StaticResource ColorReportBackground_CAP}">

                                <Border
                                    Margin="{StaticResource reportContentMargin_CAP}"
                                    Width="{StaticResource reportVerticalGuideRight_CAP}">

                                    <TextBlock
                                        Text="Longitude"
                                        FontWeight="Bold"
                                        VerticalAlignment="Center"
                                        HorizontalAlignment="Right"
                                        FontSize="{StaticResource reportFontSizeLabel_CAP}" />

                                </Border>

                                <Border
                                    Margin="{StaticResource reportContentMargin_CAP}"
                                    Background="White"
                                    Width="{StaticResource reportBoxWidth1}"
                                    BorderBrush="{StaticResource ColorReportBorder_CAP}"
                                    BorderThickness="{StaticResource reportBorderThickness_CAP}">

                                    <TextBlock
                                        Text="{Binding Path=posLonMeridianString_CAP}"
                                        HorizontalAlignment="Center"
                                        VerticalAlignment="Center"
                                        FontSize="{StaticResource reportFontSizeContent_CAP}" />

                                </Border>

                                <Border
                                    Margin="{StaticResource reportContentMargin_CAP}"
                                    Background="White"
                                    Width="{StaticResource reportBoxWidth3}"
                                    BorderBrush="{StaticResource ColorReportBorder_CAP}"
                                    BorderThickness="{StaticResource reportBorderThickness_CAP}">

                                    <TextBlock
                                        Text="{Binding Path=posLonDegreesString_CAP}"
                                        HorizontalAlignment="Center"
                                        VerticalAlignment="Center"
                                        FontSize="{StaticResource reportFontSizeContent_CAP}"/>

                                </Border>

                                <Border
                                    Margin="{StaticResource reportContentMargin_CAP}"
                                    Background="White"
                                    Width="{StaticResource reportBoxWidth4}"
                                    BorderBrush="{StaticResource ColorReportBorder_CAP}"
                                    BorderThickness="{StaticResource reportBorderThickness_CAP}">

                                    <TextBlock
                                        Text="{Binding Path=posLonMinutesString_CAP}"
                                        HorizontalAlignment="Center"
                                        VerticalAlignment="Center"
                                        FontSize="{StaticResource reportFontSizeContent_CAP}" />

                                </Border>

                            </StackPanel>

                            <!--Restricted Sector, part 1-->
                            <Grid
                                Grid.Row="4"
                                Grid.Column="0"
                                Grid.ColumnSpan="2"
                                Margin="{StaticResource reportLineMargin_CAP}"
                                Background="{StaticResource ColorReportBackground_CAP}">

                                <Border
                                    Margin="{StaticResource reportContentMargin_CAP}">

                                    <StackPanel
                                        VerticalAlignment="Center">

                                        <TextBlock
                                            Text="Restricted"
                                            FontWeight="Bold"
                                            FontSize="{StaticResource reportFontSizeLabel_CAP}"
                                            HorizontalAlignment="Left" />

                                        <TextBlock
                                            Text="Sector"
                                            FontWeight="Bold"
                                            FontSize="{StaticResource reportFontSizeLabel_CAP}"
                                            HorizontalAlignment="Left" />

                                    </StackPanel>

                                </Border>

                                <StackPanel
                                    Orientation="Horizontal">

                                    <Border
                                        Margin="{StaticResource reportContentMargin_CAP}"
                                        Width="{StaticResource reportVerticalGuideLeft_CAP}">

                                        <TextBlock
                                            Text="From"
                                            FontWeight="Bold"
                                            VerticalAlignment="Center"
