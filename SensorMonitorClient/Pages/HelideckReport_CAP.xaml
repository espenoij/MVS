                            </Grid>

                            <!--Unservicable Sensors, part 3-->
                            <Grid
                                Grid.Row="24"
                                Grid.Column="0"
                                Grid.RowSpan="2"
                                Grid.ColumnSpan="2"
                                Margin="{StaticResource reportLineMargin_CAP}">

                                <DockPanel>

                                    <Border
                                        Margin="{StaticResource reportContentMargin_CAP}">

                                        <StackPanel
                                            VerticalAlignment="Center"
                                            Width="900">

                                            <TextBlock
                                                Text="Details"
                                                FontWeight="Bold"
                                                FontSize="{StaticResource reportFontSizeLabel_CAP}"
                                                HorizontalAlignment="Right" />

                                            <TextBlock
                                                Text="Max. 3 lines"
                                                FontStyle="Italic"
                                                FontSize="{StaticResource reportFontSizeLabel_CAP}"
                                                HorizontalAlignment="Right" />

                                            <TextBlock
                                                Text="Include Dates"
                                                FontStyle="Italic"
                                                FontSize="{StaticResource reportFontSizeLabel_CAP}"
                                                HorizontalAlignment="Right" />

                                        </StackPanel>

                                    </Border>

                                    <Border
                                        Margin="{StaticResource reportRemarksPadding_CAP}"
                                        Background="White"
                                        BorderBrush="{StaticResource ColorReportBorder_CAP}"
                                        BorderThickness="{StaticResource reportBorderThickness_CAP}">

                                        <TextBlock
                                            Text="{Binding Path=anyUnserviceableSensorsCommentsString}"
                                            TextWrapping="Wrap"
                                            FontSize="{StaticResource reportFontSizeContent_CAP}"
                                            Padding="{StaticResource reportRemarksPadding_CAP}" />

                                    </Border>

                                </DockPanel>

                            </Grid>

                            <!--Remarks-->
                            <Grid
                                Grid.Row="26"
                                Grid.Column="0"
                                Grid.RowSpan="3"
                                Grid.ColumnSpan="2"
                                Margin="{StaticResource reportLineMargin_CAP}"
                                Background="{StaticResource ColorReportBackground_CAP}">

                                <DockPanel>

                                    <Border
                                        Margin="{StaticResource reportContentMargin_CAP}">

                                        <StackPanel
                                            VerticalAlignment="Center"
                                            Width="{StaticResource reportVerticalGuideLeft_CAP}">

                                            <TextBlock
                                                Text="Remarks"
                                                FontWeight="Bold"
                                                FontSize="{StaticResource reportFontSizeLabel_CAP}"
                                                HorizontalAlignment="Right" />

                                            <TextBlock
                                                Text="Max. 5 lines"
                                                FontStyle="Italic"
                                                FontSize="{StaticResource reportFontSizeLabel_CAP}"
                                                HorizontalAlignment="Right" />

                                        </StackPanel>

                                    </Border>

                                    <Border
                                        Margin="{StaticResource reportRemarksPadding_CAP}"
                                        Background="White"
                                        BorderBrush="{StaticResource ColorReportBorder_CAP}"
                                        BorderThickness="{StaticResource reportBorderThickness_CAP}">

                                        <TextBlock
                                            Text="{Binding Path=otherWeatherInfo}"
                                            TextWrapping="Wrap"
                                            FontSize="{StaticResource reportFontSizeContent_CAP}"
                                            Padding="{StaticResource reportRemarksPadding_CAP}"/>

                                    </Border>

                                </DockPanel>

                            </Grid>

                            <!--Report Prepared By-->
                            <StackPanel
                                Grid.Row="29"
                                Grid.Column="0"
                                Grid.ColumnSpan="2"
                                Orientation="Horizontal"
                                Margin="{StaticResource reportLineMargin_CAP}"
                                Background="{StaticResource ColorReportBackground_CAP}">

                                <Border
                                    Margin="{StaticResource reportContentMargin_CAP}"
                                    Width="{StaticResource reportVerticalGuideLeft_CAP}">

                                    <TextBlock
                                        Text="Report prepared by"
                                        FontWeight="Bold"
                                        VerticalAlignment="Center"
                                        HorizontalAlignment="Right"
                                        FontSize="{StaticResource reportFontSizeLabel_CAP}" />

                                </Border>

                                <Border
                                    Margin="{StaticResource reportContentMargin_CAP}"
                                    Background="White"
                                    Width="1000"
                                    BorderBrush="{StaticResource ColorReportBorder_CAP}"
                                    BorderThickness="{StaticResource reportBorderThickness_CAP}">

                                    <TextBlock
                                        Text="{Binding Path=nameOfHLO}"
                                        HorizontalAlignment="Center"
                                        VerticalAlignment="Center"
                                        FontSize="{StaticResource reportFontSizeContent_CAP}" />

                                </Border>

                            </StackPanel>

                        </Grid>

                    </Border>

                </Viewbox>

            </Grid>

        </telerik:GroupBox>

        <!--COMMANDS-->
        <telerik:GroupBox
            Header="COMMANDS"
            Grid.Column="2"
            Margin="8">

            <StackPanel>

                <telerik:RadButton
                    Name="btnSendReport"
                    Content="Send Report"
                    Margin="8"
                    HorizontalAlignment="Stretch"
                    Click="btnSendReport_Click" />

                <telerik:RadButton
                    Name="btnSavePDF"
                    Content="Save PDF File"
                    Margin="8"
                    HorizontalAlignment="Stretch"
                    Click="btnSavePDF_Click" />

                <telerik:RadButton
                    Name="btnOpenFileLocation"
                    Content="Open File Location"
                    Margin="8"
                    HorizontalAlignment="Stretch"
                    Click="btnOpenFileLocation_Click" />

            </StackPanel>

        </telerik:GroupBox>

    </Grid>

</UserControl>
