<UserControl
    x:Class="HMS_Client.HelideckReport_NOROG"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:telerik="http://schemas.telerik.com/2008/xaml/presentation"
    mc:Ignorable="d"
    d:DesignHeight="1080"
    d:DesignWidth="1920">

    <!--Advarsel! Rapporten i denne filen er designet for utskift til PDF.
    Enkelte tagger er ikke tilgjengelig i koden som skriver ut til PDF.
    Alle endringer i rapporten må sjekkes i utskrevet PDF.-->
   
    <Grid>

        <Grid.ColumnDefinitions>
            <ColumnDefinition
                Width="4*" />
            <ColumnDefinition
                Width="4*" />
            <ColumnDefinition
                Width="1*" />
        </Grid.ColumnDefinitions>

        <Grid
            Grid.Column="0">

            <Grid.ColumnDefinitions>
                <ColumnDefinition
                    Width="*" />
                <ColumnDefinition
                    Width="*" />
            </Grid.ColumnDefinitions>

            <Grid.RowDefinitions>
                <RowDefinition
                    Height="*" />
                <RowDefinition
                    Height="*" />
                <RowDefinition
                    Height="*" />
            </Grid.RowDefinitions>

            <!--HELIDECK REPORT-->
            <telerik:GroupBox
                Header="HELIDECK REPORT"
                Grid.Row="0"
                Margin="8">

                <DockPanel>

                    <telerik:RadButton
                        DockPanel.Dock="Bottom"
                        Name="btnClearHelideckReport"
                        Content="Clear"
                        HorizontalAlignment="Right"
                        Width="110"
                        Click="btnClearHelideckReport_Click" />

                    <StackPanel
                        DockPanel.Dock="Top"
                        Margin="8">

                        <!--Name of HLO-->
                        <DockPanel>

                            <telerik:Label
                                Width="130"
                                Content="Name of HLO"
                                VerticalAlignment="Center" />

                            <TextBox
                                Name="tbNameOfHLO"
                                Text="{Binding Path=nameOfHLO}"
                                LostFocus="tbNameOfHLO_LostFocus"
                                KeyDown="tbNameOfHLO_KeyDown"/>

                        </DockPanel>

                        <!--Email-->
                        <DockPanel>

                            <telerik:Label
                                Width="130"
                                Content="Email: From"
                                VerticalAlignment="Center" />

                            <TextBox
                                Name="tbEmail"
                                Text="{Binding Path=emailFrom}"
                                LostFocus="tbEmail_LostFocus"
                                KeyDown="tbEmail_KeyDown" />

                        </DockPanel>

                        <!--Telephone-->
                        <DockPanel>

                            <telerik:Label
                                Width="130"
                                Content="Telephone"
                                VerticalAlignment="Center" />

                            <TextBox
                                Name="tbTelephone"
                                Text="{Binding Path=telephone}"
                                LostFocus="tbTelephone_LostFocus"
                                KeyDown="tbTelephone_KeyDown" />

                        </DockPanel>

                        <!--Dynamic Positioning-->
                        <DockPanel>

                            <telerik:Label
                                Name="lbDynamicPositioning"
                                Content="Dynamic Positioning"
                                VerticalAlignment="Center" />

                            <CheckBox
                                Name="cbDynamicPositioning"
                                IsChecked="{Binding Path=dynamicPositioning, Mode=TwoWay}"
                                VerticalAlignment="Center"
                                HorizontalAlignment="Right" />

                        </DockPanel>

                        <!--Accurate Monitoring Equipment-->
                        <DockPanel>

                            <telerik:Label
                                Name="lbAccurateMonitoringEquipment"
                                Content="Accurate Monitoring Equipment"
                                VerticalAlignment="Center" />

                            <CheckBox
                                Name="cbAccurateMonitoringEquipment"
                                IsChecked="{Binding Path=accurateMonitoringEquipment, Mode=TwoWay}"
                                VerticalAlignment="Center"
                                HorizontalAlignment="Right" />

                        </DockPanel>

                    </StackPanel>

                </DockPanel>

            </telerik:GroupBox>

            <!--LOG INFO-->
            <telerik:GroupBox
                Header="LOG INFO"
                Grid.Column="1"
                Grid.Row="0"
                Grid.RowSpan="3"
                Margin="8">

                <DockPanel>

                    <telerik:RadButton
                        DockPanel.Dock="Bottom"
                        Name="btnClearLogInfo"
                        Content="Clear"
                        HorizontalAlignment="Right"
                        Width="110" 
                        Click="btnClearLogInfo_Click"/>

                    <StackPanel
                        Margin="8"
                        DockPanel.Dock="Top">

                        <!--Flight Number-->
                        <DockPanel>

                            <telerik:Label
                                Width="200"
                                Content="Flight Number"
                                VerticalAlignment="Center" />

                            <TextBox
                                Name="tbFlightNumber"
                                Text="{Binding Path=flightNumber}"
                                LostFocus="tbFlightNumber_LostFocus"
                                KeyDown="tbFlightNumber_KeyDown" />

                        </DockPanel>

                        <!--Return Load-->
                        <DockPanel>

                            <telerik:Label
                                Width="200"
                                Content="Return Load (passengers)"
                                VerticalAlignment="Center" />

                            <TextBox
                                Name="tbReturnLoad"
                                Text="{Binding Path=returnLoad}"
                                LostFocus="tbReturnLoad_LostFocus"
                                KeyDown="tbReturnLoad_KeyDown" />

                        </DockPanel>

                        <!--Total Load-->
                        <DockPanel>

                            <telerik:Label
                                Width="200"
                                Content="Total Load"
                                VerticalAlignment="Center" />

                            <TextBox
                                Name="tbTotalLoad"
                                Text="{Binding Path=totalLoad}"
                                LostFocus="tbTotalLoad_LostFocus"
                                KeyDown="tbTotalLoad_KeyDown" />

                        </DockPanel>

                        <!--Luggage-->
                        <DockPanel>

                            <telerik:Label
                                Width="200"
                                Content="Luggage"
                                VerticalAlignment="Center" />

                            <TextBox
                                Name="tbLuggage"
                                Text="{Binding Path=luggage}"
                                LostFocus="tbLuggage_LostFocus"
                                KeyDown="tbLuggage_KeyDown" />

                        </DockPanel>

                        <!--Cargo-->
                        <DockPanel>

                            <telerik:Label
                                Width="200"
                                Content="Cargo"
                                VerticalAlignment="Center" />

                            <TextBox
                                Name="tbCargo"
                                Text="{Binding Path=cargo}"
                                LostFocus="tbCargo_LostFocus"
                                KeyDown="tbCargo_KeyDown" />

                        </DockPanel>

                        <!--Helifuel Available-->
                        <DockPanel>

                            <telerik:Label
                                Content="Helifuel Available"
                                VerticalAlignment="Center" />

                            <CheckBox
                                IsChecked="{Binding Path=helifuelAvailable, Mode=TwoWay}"
                                VerticalAlignment="Center"
                                HorizontalAlignment="Right" />

                        </DockPanel>

                        <!--Fuel Quantity-->
                        <DockPanel>

                            <telerik:Label
                                Width="200"
                                Content="Fuel Quantity"
                                VerticalAlignment="Center" />

                            <TextBox
                                Name="tbFuelQuantity"
                                Text="{Binding Path=fuelQuantity}"
                                LostFocus="tbFuelQuantity_LostFocus"
                                KeyDown="tbFuelQuantity_KeyDown" />

                        </DockPanel>

                        <!--Routing 1-->
                        <DockPanel>

                            <telerik:Label
                                Width="200"
                                Content="Routing 1"
                                VerticalAlignment="Center" />

                            <TextBox
                                Name="tbRouting1"
                                Text="{Binding Path=routing1}"
                                LostFocus="tbRouting1_LostFocus"
                                KeyDown="tbRouting1_KeyDown" />

                        </DockPanel>

                        <!--Routing 2-->
                        <DockPanel>

                            <telerik:Label
                                Width="200"
                                Content="Routing 2"
                                VerticalAlignment="Center" />

                            <TextBox
                                Name="tbRouting2"
                                Text="{Binding Path=routing2}"
                                LostFocus="tbRouting2_LostFocus"
                                KeyDown="tbRouting2_KeyDown" />

                        </DockPanel>

                        <!--Routing 3-->
                        <DockPanel>

                            <telerik:Label
                                Width="200"
                                Content="Routing 3"
                                VerticalAlignment="Center" />

                            <TextBox
                                Name="tbRouting3"
                                Text="{Binding Path=routing3}"
                                LostFocus="tbRouting3_LostFocus"
                                KeyDown="tbRouting3_KeyDown" />

                        </DockPanel>

                        <!--Routing 4-->
                        <DockPanel>

                            <telerik:Label
                                Width="200"
                                Content="Routing 4"
                                VerticalAlignment="Center" />

                            <TextBox
                                Name="tbRouting4"
                                Text="{Binding Path=routing4}"
                                LostFocus="tbRouting4_LostFocus"
                                KeyDown="tbRouting4_KeyDown" />

                        </DockPanel>

                        <telerik:Label
                            Content="Special Remarks"
                            HorizontalAlignment="Left" />

                        <TextBlock
                            DockPanel.Dock="Top"
                            Margin="16,0,16,0"
                            Text="E.g. ships within the 500m sone, non-conformities of helideck equipment, objects close the obstruction free zone, deviation from helideck procedures, flaring of gas, or other information the pilots may find useful."
                            FontSize="{StaticResource reportFontSizeComment}"
                            TextWrapping="Wrap"
                            VerticalAlignment="Center" />

                        <!--<Grid
                            Height="100">

                            <TextBox
                                Name="tbLogInfoRemarks"
                                Margin="8"
                                Text="{Binding Path=logInfoRemarks}"
                                TextWrapping="Wrap"
                                VerticalAlignment="Bottom"
                                LostFocus="tbLogInfoRemarks_LostFocus"
                                KeyDown="tbLogInfoRemarks_KeyDown"/>
                        </Grid>-->


                        <Grid
                            Height="100">

                            <TextBox
                                x:Name="SpecialRemarksBox"
                                Margin="8"
                                Text="{Binding Path=logInfoRemarks}"
                                TextWrapping="Wrap"
                                VerticalAlignment="Bottom"
                                LostFocus="tbLogInfoRemarks_LostFocus"
                                KeyDown="tbLogInfoRemarks_KeyDown" />

                            <TextBlock
                                IsHitTestVisible="False"
                                Text="Enter special remarks here"
                                VerticalAlignment="Bottom"
                                HorizontalAlignment="Left"
                                Margin="18,0,0,18"
                                Foreground="DarkGray">

                                <TextBlock.Style>
                                    <Style
                                        TargetType="{x:Type TextBlock}">
                                        <Setter
                                            Property="Visibility"
                                            Value="Collapsed" />
                                        <Style.Triggers>
                                            <DataTrigger
                                                Binding="{Binding Text, ElementName=SpecialRemarksBox}"
                                                Value="">
                                                <Setter
                                                    Property="Visibility"
                                                    Value="Visible" />
                                            </DataTrigger>
                                        </Style.Triggers>
                                    </Style>
                                </TextBlock.Style>
                            </TextBlock>
                        </Grid>

                    </StackPanel>

                </DockPanel>

            </telerik:GroupBox>

            <!--WEATHER OBSERVATION-->
            <telerik:GroupBox
                Header="WEATHER OBSERVATION"
                Grid.Column="0"
                Grid.Row="1"
                Margin="8">

                <DockPanel>

                    <telerik:RadButton
                        DockPanel.Dock="Bottom"
                        Name="btnClearOtherWeatherInfo"
                        Content="Clear"
                        HorizontalAlignment="Right"
                        Width="110"
                        Click="btnClearOtherWeatherInfo_Click"/>

                    <StackPanel
                        DockPanel.Dock="Top"
                        Margin="8">

                        <!--Sea spray observed over helideck-->
                        <DockPanel>

                            <telerik:Label
                                Content="Sea spray observed over helideck"
                                VerticalAlignment="Center" />

                            <CheckBox
                                IsChecked="{Binding Path=seaSprayObserved, Mode=TwoWay}"
                                VerticalAlignment="Center"
                                HorizontalAlignment="Right" />

                        </DockPanel>

                        <telerik:Label
                            Content="Other relevant weather info"
                            HorizontalAlignment="Left" />

                        <TextBlock
                            Margin="16,0,16,0"
                            Text=" E.g. fog banks, rapid changes, etc."
                            FontSize="{StaticResource reportFontSizeComment}"
                            TextWrapping="Wrap"
                            VerticalAlignment="Center" />

                        <Grid
                            Height="100">
                            
                            <TextBox
                                Name="tbOtherWeatherInfo"
                                Margin="8"
                                Text="{Binding Path=otherWeatherInfo}"
                                TextWrapping="Wrap"
                                VerticalAlignment="Bottom"
                                LostFocus="tbOtherWeatherInfo_LostFocus"
                                KeyDown="tbOtherWeatherInfo_KeyDown"/>

                            <TextBlock
                                IsHitTestVisible="False"
                                Text="Enter weather info here"
                                VerticalAlignment="Bottom"
                                HorizontalAlignment="Left"
                                Margin="18,0,0,18"
                                Foreground="DarkGray">

                                <TextBlock.Style>
                                    <Style
                                        TargetType="{x:Type TextBlock}">
                                        <Setter
                                            Property="Visibility"
                                            Value="Collapsed" />
                                        <Style.Triggers>
                                            <DataTrigger
                                                Binding="{Binding Text, ElementName=tbOtherWeatherInfo}"
                                                Value="">
                                                <Setter
                                                    Property="Visibility"
                                                    Value="Visible" />
                                            </DataTrigger>
                                        </Style.Triggers>
                                    </Style>
                                </TextBlock.Style>
                            </TextBlock>
                        </Grid>

                    </StackPanel>

                </DockPanel>

            </telerik:GroupBox>

            <!--EMAIL-->
            <telerik:GroupBox
                Header="EMAIL"
                Grid.Column="0"
                Grid.Row="2"
                Margin="8">

                <DockPanel>

                    <telerik:RadButton
                        DockPanel.Dock="Bottom"
                        Name="btnClearEmail"
                        Content="Clear"
                        HorizontalAlignment="Right"
                        Width="110"
                        Click="btnClearEmail_Click"/>

                    <StackPanel
                        DockPanel.Dock="Top"
                        Margin="8">

                        <!--Helicopter Operator-->
                        <DockPanel>
                            <telerik:Label
                                Content="Helicopter Operator"
                                Width="150"
                                VerticalAlignment="Center" />
                            <telerik:RadComboBox
                                Name="cboHelicopterOperator"
                                SelectionChanged="cboHelicopterOperator_SelectionChanged">
                            </telerik:RadComboBox>
                        </DockPanel>

                        <!--Send To-->
                        <DockPanel>

                            <telerik:Label
                                Width="150"
                                Content="Email: To"
                                VerticalAlignment="Center" />

                            <TextBox
                                Name="tbSendTo"
                                Text="{Binding Path=emailTo}"
                                LostFocus="tbSendTo_LostFocus"
                                KeyDown="tbSendTo_KeyDown"/>

                        </DockPanel>

                        <!--CC To-->
                        <DockPanel>

                            <telerik:Label
                                Width="150"
                                Content="Email: CC"
                                VerticalAlignment="Center" />

                            <TextBox
                                Name="tbCCTo"
                                Text="{Binding Path=emailCC}"
                                LostFocus="tbCCTo_LostFocus"
                                KeyDown="tbCCTo_KeyDown"/>

                        </DockPanel>

                        <!--Include HMS screenshot-->
                        <DockPanel>

                            <telerik:Label
                                Content="Include HMS screen capture"
                                VerticalAlignment="Center" />

                            <CheckBox
                                IsChecked="{Binding Path=sendHMSScreenCapture, Mode=TwoWay}"
                                VerticalAlignment="Center"
                                HorizontalAlignment="Right" />

                        </DockPanel>

                    </StackPanel>

                </DockPanel>

            </telerik:GroupBox>

        </Grid>

        <!--REPORT PREVIEW-->
        <telerik:GroupBox
            Header="PREVIEW"
            Grid.Column="1"
            Margin="8">

            <Grid>

                <Viewbox
                    Grid.Column="1"
                    Margin="8"
                    StretchDirection="Both"
                    Stretch="Uniform">

                    <Border
                        x:Name="helideckReportPreviewContainer"
                        Height="3508"
                        Width="2480">

                        <Grid
                            Margin="40">

                            <Grid.RowDefinitions>
                                <RowDefinition
                                    Height="3*" />
                                <RowDefinition
                                    Height="48" />
                                <RowDefinition
                                    Height="2*" />
                                <RowDefinition
                                    Height="48" />
                                <RowDefinition
                                    Height="1*" />
                            </Grid.RowDefinitions>

                            <!--General Information-->
                            <Grid
                                Grid.Row="0">

                                <Grid.RowDefinitions>
                                    <RowDefinition
                                        Height="3*" />
                                    <RowDefinition
                                        Height="*" />
                                    <RowDefinition
                                        Height="*" />
                                    <RowDefinition
                                        Height="*" />
                                    <RowDefinition
                                        Height="*" />
                                    <RowDefinition
                                        Height="*" />
                                    <RowDefinition
                                        Height="*" />
                                    <RowDefinition
                                        Height="*" />
                                    <RowDefinition
                                        Height="*" />
                                    <RowDefinition
                                        Height="2*" />
                                    <RowDefinition
                                        Height="*" />
                                </Grid.RowDefinitions>

                                <Grid
                                    Grid.Row="0">

                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition
                                            Width="*" />
                                        <ColumnDefinition
                                            Width="*" />
                                    </Grid.ColumnDefinitions>

                                    <Rectangle
                                        Grid.Column="0"
                                        Fill="{StaticResource ColorReportLines}"
                                        Width="{StaticResource reportStrokeThickness}"
                                        HorizontalAlignment="Right"
                                        VerticalAlignment="Stretch" />

                                    <Rectangle
                                        Grid.Column="0"
                                        Grid.ColumnSpan="2"
                                        Fill="{StaticResource ColorReportLines}"
                                        Height="{StaticResource reportStrokeThickness}"
                                        VerticalAlignment="Bottom"
                                        HorizontalAlignment="Stretch" />

                                    <!--General Information: Title-->
                                    <TextBlock
                                        Text="HELIDECK REPORT"
                                        VerticalAlignment="Center"
                                        HorizontalAlignment="Center"
                                        FontSize="{StaticResource reportFontSizeTitle1}" />

                                    <!--General Information: Installation, Email, Telephone-->
                                    <Grid
                                        Grid.Column="1">

                                        <Grid.RowDefinitions>
                                            <RowDefinition
                                                Height="*" />
                                            <RowDefinition
                                                Height="*" />
                                            <RowDefinition
                                                Height="*" />
                                        </Grid.RowDefinitions>

                                        <!--General Information: Installation-->
                                        <Rectangle
                                            Grid.Row="0"
                                            Fill="{StaticResource ColorReportLines}"
                                            Height="{StaticResource reportStrokeThickness}"
                                            VerticalAlignment="Bottom"
                                            HorizontalAlignment="Stretch" />

                                        <StackPanel
                                            Grid.Row="0"
                                            Orientation="Horizontal">

                                            <TextBlock
                                                Text="Installation:"
                                                VerticalAlignment="Bottom"
                                                HorizontalAlignment="Left"
                                                Margin="{StaticResource reportLabelMargin}"
                                                FontSize="{StaticResource reportFontSizeLabel}" />

                                            <TextBlock
                                                Text="{Binding Path=vesselNameString}"
                                                VerticalAlignment="Bottom"
                                                HorizontalAlignment="Left"
                                                FontSize="{StaticResource reportFontSizeContent}"
                                                Margin="{StaticResource reportContentMargin}" />

                                        </StackPanel>

                                        <!--General Information: Email-->
                                        <Rectangle
                                            Grid.Row="1"
                                            Fill="{StaticResource ColorReportLines}"
                                            Height="{StaticResource reportStrokeThickness}"
                                            VerticalAlignment="Bottom"
                                            HorizontalAlignment="Stretch" />

                                        <StackPanel
                                            Grid.Row="1"
                                            Orientation="Horizontal">

                                            <TextBlock
                                                Text="Email:"
                                                VerticalAlignment="Bottom"
                                                HorizontalAlignment="Left"
                                                Margin="{StaticResource reportLabelMargin}"
                                                FontSize="{StaticResource reportFontSizeLabel}" />

                                            <TextBlock
                                                Text="{Binding Path=emailFrom}"
                                                VerticalAlignment="Bottom"
                                                HorizontalAlignment="Left"
                                                FontSize="{StaticResource reportFontSizeContent}"
                                                Margin="{StaticResource reportContentMargin}" />

                                        </StackPanel>

                                        <!--General Information: Telephone-->
                                        <StackPanel
                                            Grid.Row="2"
                                            Orientation="Horizontal">

                                            <TextBlock
                                                Text="Tel:"
                                                VerticalAlignment="Bottom"
                                                HorizontalAlignment="Left"
                                                Margin="{StaticResource reportLabelMargin}"
                                                FontSize="{StaticResource reportFontSizeLabel}" />

                                            <TextBlock
                                                Text="{Binding Path=telephone}"
                                                VerticalAlignment="Bottom"
                                                HorizontalAlignment="Left"
                                                FontSize="{StaticResource reportFontSizeContent}"
                                                Margin="{StaticResource reportContentMargin}" />

                                        </StackPanel>

                                    </Grid>

                                </Grid>

                                <!--General Information: Date, Time, Position-->
                                <Grid
                                    Grid.Row="1">

                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition
                                            Width="3*" />
                                        <ColumnDefinition
                                            Width="3*" />
                                        <ColumnDefinition
                                            Width="6*" />
                                    </Grid.ColumnDefinitions>

                                    <Rectangle
                                        Grid.Column="0"
                                        Grid.ColumnSpan="3"
                                        Fill="{StaticResource ColorReportLines}"
                                        Height="{StaticResource reportStrokeThickness}"
                                        VerticalAlignment="Bottom"
                                        HorizontalAlignment="Stretch" />

                                    <Rectangle
                                        Grid.Column="0"
                                        Fill="{StaticResource ColorReportLines}"
                                        Width="{StaticResource reportStrokeThickness}"
                                        HorizontalAlignment="Right"
                                        VerticalAlignment="Stretch" />

                                    <Rectangle
                                        Grid.Column="1"
                                        Fill="{StaticResource ColorReportLines}"
                                        Width="{StaticResource reportStrokeThickness}"
                                        HorizontalAlignment="Right"
                                        VerticalAlignment="Stretch" />

                                    <!--General Information: Date-->
                                    <StackPanel
                                        Grid.Column="0"
                                        Orientation="Horizontal">

                                        <TextBlock
                                            Text="Date:"
                                            VerticalAlignment="Bottom"
                                            HorizontalAlignment="Left"
                                            Margin="{StaticResource reportLabelMargin}"
                                            FontSize="{StaticResource reportFontSizeLabel}" />

                                        <TextBlock
                                            Text="{Binding Path=dateString_NOROG}"
                                            VerticalAlignment="Bottom"
                                            HorizontalAlignment="Left"
                                            FontSize="{StaticResource reportFontSizeContent}"
                                            Margin="{StaticResource reportContentMargin}" />

                                    </StackPanel>

                                    <!--General Information: Time-->
                                    <StackPanel
                                        Grid.Column="1"
                                        Orientation="Horizontal">

                                        <TextBlock
                                            Text="Time (UTC):"
                                            VerticalAlignment="Bottom"
                                            HorizontalAlignment="Left"
                                            Margin="{StaticResource reportLabelMargin}"
                                            FontSize="{StaticResource reportFontSizeLabel}" />

                                        <TextBlock
                                            Text="{Binding Path=clockString_NOROG}"
                                            VerticalAlignment="Bottom"
                                            HorizontalAlignment="Left"
                                            FontSize="{StaticResource reportFontSizeContent}"
                                            Margin="{StaticResource reportContentMargin}" />

                                    </StackPanel>

                                    <!--General Information: Position-->
                                    <StackPanel
                                        Grid.Column="2"
                                        Orientation="Horizontal">

                                        <TextBlock
                                            Text="Position:"
                                            VerticalAlignment="Bottom"
                                            HorizontalAlignment="Left"
                                            Margin="{StaticResource reportLabelMargin}"
                                            FontSize="{StaticResource reportFontSizeLabel}" />

                                        <TextBlock
                                            Text="{Binding Path=positionString_NOROG}"
                                            VerticalAlignment="Bottom"
                                            HorizontalAlignment="Left"
                                            FontSize="{StaticResource reportFontSizeContent}"
                                            Margin="{StaticResource reportContentMargin}" />

                                    </StackPanel>

                                </Grid>

                                <!--General Information: Dynamic Positioning, NDB-->
                                <Grid
                                    Grid.Row="2">

                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition
                                            Width="3.5*" />
                                        <ColumnDefinition
                                            Width="2.5*" />
                                        <ColumnDefinition
                                            Width="6*" />
                                    </Grid.ColumnDefinitions>

                                    <Rectangle
                                        Grid.Column="0"
                                        Grid.ColumnSpan="3"
                                        Fill="{StaticResource ColorReportLines}"
                                        Height="{StaticResource reportStrokeThickness}"
                                        VerticalAlignment="Bottom"
                                        HorizontalAlignment="Stretch" />

                                    <Rectangle
                                        Grid.Column="1"
                                        Fill="{StaticResource ColorReportLines}"
                                        Width="{StaticResource reportStrokeThickness}"
                                        HorizontalAlignment="Right"
                                        VerticalAlignment="Stretch" />

                                    <!--General Information: Dynamic Positioning-->
                                    <TextBlock
                                        Grid.Column="0"
                                        Text="Dynamic positioning:"
                                        VerticalAlignment="Bottom"
                                        HorizontalAlignment="Left"
                                        Margin="{StaticResource reportLabelMargin}"
                                        FontSize="{StaticResource reportFontSizeLabel}" />

                                    <StackPanel
                                        Grid.Column="1"
                                        Orientation="Horizontal">

                                        <!--YES-->
                                        <Grid
                                            VerticalAlignment="Center"
                                            Margin="{StaticResource reportRadioButtonMargin}">

                                            <Ellipse
                                                VerticalAlignment="Center"
                                                HorizontalAlignment="Center"
                                                Width="{StaticResource reportRadioButtonSizeOuter}"
                                                Height="{StaticResource reportRadioButtonSizeOuter}"
                                                StrokeThickness="{StaticResource reportRadioButtonStroke}"
                                                Stroke="{StaticResource ColorReportLines}" />

                                            <Ellipse
                                                Visibility="{Binding Path=dynamicPositioningYes}"
                                                VerticalAlignment="Center"
                                                HorizontalAlignment="Center"
                                                Width="{StaticResource reportRadioButtonSizeInner}"
                                                Height="{StaticResource reportRadioButtonSizeInner}"
                                                Fill="{StaticResource ColorReportLines}" />

                                        </Grid>

                                        <TextBlock
                                            Text="Yes"
                                            VerticalAlignment="Bottom"
                                            HorizontalAlignment="Left"
                                            Margin="{StaticResource reportLabelMargin}"
                                            FontSize="{StaticResource reportFontSizeLabel}" />

                                        <!--NO-->
                                        <Grid
                                            VerticalAlignment="Center"
                                            Margin="{StaticResource reportRadioButtonMargin}">

                                            <Ellipse
                                                VerticalAlignment="Center"
                                                HorizontalAlignment="Center"
                                                Width="{StaticResource reportRadioButtonSizeOuter}"
                                                Height="{StaticResource reportRadioButtonSizeOuter}"
                                                StrokeThickness="{StaticResource reportRadioButtonStroke}"
                                                Stroke="{StaticResource ColorReportLines}" />

                                            <Ellipse
                                                Visibility="{Binding Path=dynamicPositioningNo}"
                                                VerticalAlignment="Center"
                                                HorizontalAlignment="Center"
                                                Width="{StaticResource reportRadioButtonSizeInner}"
                                                Height="{StaticResource reportRadioButtonSizeInner}"
                                                Fill="{StaticResource ColorReportLines}" />

                                        </Grid>

                                        <TextBlock
                                            Text="No"
                                            VerticalAlignment="Bottom"
                                            HorizontalAlignment="Left"
                                            Margin="{StaticResource reportLabelMargin}"
                                            FontSize="{StaticResource reportFontSizeLabel}" />

                                    </StackPanel>

                                    <!--General Information: NDB-->
                                    <StackPanel
                                        Grid.Column="2"
                                        Orientation="Horizontal">

                                        <TextBlock
                                            Text="NDB:"
                                            VerticalAlignment="Bottom"
                                            HorizontalAlignment="Left"
                                            Margin="{StaticResource reportLabelMargin}"
                                            FontSize="{StaticResource reportFontSizeLabel}" />

                                        <TextBlock
                                            Text="{Binding Path=ndbFreqString_NOROG}"
                                            VerticalAlignment="Bottom"
                                            HorizontalAlignment="Left"
                                            FontSize="{StaticResource reportFontSizeContent}"
                                            Margin="{StaticResource reportContentMargin}" />

                                    </StackPanel>

                                    <!--kHz-->
                                    <TextBlock
                                        Grid.Column="2"
                                        Text="kHz"
                                        VerticalAlignment="Bottom"
                                        HorizontalAlignment="Right"
