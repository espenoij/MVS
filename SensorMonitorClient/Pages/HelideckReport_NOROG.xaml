                                            VerticalAlignment="Bottom"
                                            HorizontalAlignment="Left"
                                            FontSize="{StaticResource reportFontSizeContent}"
                                            Margin="{StaticResource reportContentMargin}" />

                                    </StackPanel>

                                    <!--Helideck Movement: Roll Left-->
                                    <StackPanel
                                        Grid.Column="2"
                                        Orientation="Horizontal">

                                        <TextBlock
                                            Text="Roll&#x0a;left:"
                                            VerticalAlignment="Bottom"
                                            HorizontalAlignment="Left"
                                            Margin="{StaticResource reportLabelMargin}"
                                            FontSize="{StaticResource reportFontSizeLabel}" />

                                        <TextBlock
                                            Text="{Binding Path=rollLeftString}"
                                            VerticalAlignment="Bottom"
                                            HorizontalAlignment="Left"
                                            FontSize="{StaticResource reportFontSizeContent}"
                                            Margin="{StaticResource reportContentMargin}" />

                                    </StackPanel>

                                    <!--Helideck Movement: Roll Right-->
                                    <StackPanel
                                        Grid.Column="3"
                                        Orientation="Horizontal">

                                        <TextBlock
                                            Text="Roll&#x0a;right:"
                                            VerticalAlignment="Bottom"
                                            HorizontalAlignment="Left"
                                            Margin="{StaticResource reportLabelMargin}"
                                            FontSize="{StaticResource reportFontSizeLabel}" />

                                        <TextBlock
                                            Text="{Binding Path=rollRightString}"
                                            VerticalAlignment="Bottom"
                                            HorizontalAlignment="Left"
                                            FontSize="{StaticResource reportFontSizeContent}"
                                            Margin="{StaticResource reportContentMargin}" />

                                    </StackPanel>

                                    <!--Helideck Movement: Heave Period-->
                                    <StackPanel
                                        Grid.Column="4"
                                        Orientation="Horizontal">

                                        <TextBlock
                                            Text="Heave period (if available):"
                                            VerticalAlignment="Bottom"
                                            HorizontalAlignment="Left"
                                            Margin="{StaticResource reportLabelMargin}"
                                            FontSize="{StaticResource reportFontSizeLabel}" />

                                        <TextBlock
                                            Text="{Binding Path=heavePeriodString}"
                                            VerticalAlignment="Bottom"
                                            HorizontalAlignment="Left"
                                            FontSize="{StaticResource reportFontSizeContent}"
                                            Margin="{StaticResource reportContentMargin}" />

                                    </StackPanel>

                                    <!--Sec.-->
                                    <TextBlock
                                        Grid.Column="4"
                                        Text="Sec."
                                        VerticalAlignment="Bottom"
                                        HorizontalAlignment="Right"
                                        Margin="{StaticResource reportLabelMargin}"
                                        Width="{StaticResource reportUnitWidth}"
                                        FontSize="{StaticResource reportFontSizeLabel}" />

                                </Grid>

                                <!--Helideck Movement: Inclination, max heave rate-->
                                <Grid
                                    Grid.Row="3">

                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition
                                            Width="4*" />
                                        <ColumnDefinition
                                            Width="3*" />
                                    </Grid.ColumnDefinitions>

                                    <Rectangle
                                        Grid.Column="0"
                                        Fill="{StaticResource ColorReportLines}"
                                        Width="{StaticResource reportStrokeThickness}"
                                        HorizontalAlignment="Right"
                                        VerticalAlignment="Stretch" />

                                    <!--Helideck Movement: Max inclination-->
                                    <StackPanel
                                        Grid.Column="0"
                                        Orientation="Horizontal">

                                        <TextBlock
                                            Text="Max helideck inclination:"
                                            VerticalAlignment="Bottom"
                                            HorizontalAlignment="Left"
                                            Margin="{StaticResource reportLabelMargin}"
                                            FontSize="{StaticResource reportFontSizeLabel}" />

                                        <TextBlock
                                            Text="{Binding Path=inclinationString}"
                                            VerticalAlignment="Bottom"
                                            HorizontalAlignment="Left"
                                            FontSize="{StaticResource reportFontSizeContent}"
                                            Margin="{StaticResource reportContentMargin}" />

                                    </StackPanel>


                                    <!--Helideck Movement: Max heave rate-->
                                    <StackPanel
                                        Grid.Column="1"
                                        Orientation="Horizontal">

                                        <TextBlock
                                            Text="Max heave rate (if available):"
                                            VerticalAlignment="Bottom"
                                            HorizontalAlignment="Left"
                                            Margin="{StaticResource reportLabelMargin}"
                                            FontSize="{StaticResource reportFontSizeLabel}" />

                                        <TextBlock
                                            Text="{Binding Path=maximumHeaveRateString}"
                                            VerticalAlignment="Bottom"
                                            HorizontalAlignment="Left"
                                            FontSize="{StaticResource reportFontSizeContent}"
                                            Margin="{StaticResource reportContentMargin}" />

                                    </StackPanel>

                                    <!--m/s-->
                                    <TextBlock
                                        Grid.Column="2"
                                        Text="m/s"
                                        VerticalAlignment="Bottom"
                                        HorizontalAlignment="Right"
                                        Margin="{StaticResource reportLabelMargin}"
                                        Width="{StaticResource reportUnitWidth}"
                                        FontSize="{StaticResource reportFontSizeLabel}" />

                                </Grid>

                            </Grid>

                        <!--Bokser rundt hovedseksjonenen-->
                        <Rectangle
                            Grid.Row="0"
                            Stroke="{StaticResource ColorReportLines}"
                            StrokeThickness="{StaticResource reportStrokeThickness}" />

                        <Rectangle
                            Grid.Row="2"
                            Stroke="{StaticResource ColorReportLines}"
                            StrokeThickness="{StaticResource reportStrokeThickness}" />

                        <Rectangle
                            Grid.Row="5"
                            Stroke="{StaticResource ColorReportLines}"
                            StrokeThickness="{StaticResource reportStrokeThickness}" />

                        </Grid>

                    </Border>

                </Viewbox>

            </Grid>

        </telerik:GroupBox>

        <!--COMMANDS-->
        <telerik:GroupBox
            Header="COMMANDS"
            Grid.Column="2"
            Margin="8">

            <StackPanel>

                <telerik:RadButton
                    Name="btnSendReport"
                    Content="Send Report"
                    Margin="8"
                    HorizontalAlignment="Stretch"
                    Click="btnSendReport_Click"/>

                <telerik:RadButton
                    Name="btnSavePDF"
                    Content="Save PDF File"
                    Margin="8"
                    HorizontalAlignment="Stretch"
                    Click="btnSavePDF_Click" />

                <telerik:RadButton
                    Name="btnOpenFileLocation"
                    Content="Open File Location"
                    Margin="8"
                    HorizontalAlignment="Stretch"
                    Click="btnOpenFileLocation_Click" />

            </StackPanel>

        </telerik:GroupBox>

    </Grid>

</UserControl>
